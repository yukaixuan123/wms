{"ast":null,"code":"import $http from \"@/http/base\";\nexport function formatDate(date) {\n  let time = new Date(date);\n  console.log(time);\n  let year = time.getFullYear();\n  console.log(year);\n  let month = time.getMonth() + 1;\n  month = month.toString().padStart(2, \"0\");\n  let day = time.getDate().toString().padStart(2, \"0\");\n  return year + \"-\" + month + \"-\" + day + \"\";\n}\nexport default {\n  data() {\n    return {\n      activeIndex: \"1\",\n      zdr: \"\",\n      date: \"\",\n      djzt: \"\",\n      gys: \"\",\n      date1: \"\",\n      lxr: \"\",\n      fs: \"\",\n      th: false,\n      jhr: \"\",\n      thyy: \"\",\n      bz: \"\",\n      buy_code: \"\",\n      tableData: [],\n      tableData1: [],\n      tableData2: [],\n      multipleSelection: [],\n      yrksl: 0,\n      drksl: 0,\n      hpsl: 0,\n      zje: 0,\n      rksl: 0,\n      rkje: 0,\n      activeName: \"first\",\n      thsl: 0,\n      zje1: 0,\n      thje: 0\n    };\n  },\n  methods: {\n    delAll() {\n      this.$router.push({\n        path: \"/SalesReturnsTH\",\n        query: {\n          sr_code: this.$route.query.sr_code\n        }\n      });\n    },\n    sgb() {\n      this.$router.go(-1);\n    },\n    //关闭\n    gb() {\n      this.$router.go(-1);\n    },\n    fzcg() {\n      this.$message({\n        message: \"复制成功\",\n        type: \"success\"\n      });\n    },\n    //下标\n    handleSelect(key, keyPath) {\n      console.log(key, keyPath);\n    },\n    //选择\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n    //根据id查退货订单\n    findId() {\n      $http.get(\"http://localhost:5149/api/Wh_sale_return_h/Wh_sale_return_h_CodeAsyunc?sr_code=\" + this.$route.query.sr_code + \"\").then(res => {\n        console.log(res);\n        this.buy_code = res.buy_code;\n        this.zdr = res.create_by;\n        this.date = res.create_time;\n        this.djzt = res.sale_state;\n        if (this.djzt == 2) {}\n        this.gys = res.cust_code;\n        this.date1 = res.sr_date;\n        this.lxr = res.link_man;\n        this.fs = res.link_way;\n        this.jhr = res.sale_duty_man;\n        this.thyy = res.remark;\n        this.bz = res.remarks;\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    //退货明细\n    find() {\n      $http.get(\"http://localhost:5149/api/Wh_sale_h/Wh_sale_h_Code1Asyunc?sale_code=\" + this.$route.query.sale_code + \"\").then(res => {\n        this.tableData = res;\n        this.FindNUm();\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    FindNUm() {\n      $http.get(\"http://localhost:5149/api/Wh_sale_return_h/Wh_sale_return_h_Code1Asyunc?sr_code=\" + this.$route.query.sr_code + \"\").then(res => {\n        let i = -1;\n        this.tableData = this.tableData.map(item => {\n          i++;\n          return {\n            ...item,\n            return_num: res[i].return_num,\n            return_money: res[i].return_money\n          };\n        });\n        console.log(this.tableData);\n        for (let i = 0; i < res.length; i++) {\n          this.thsl += res[i].return_num;\n          this.thje += res[i].return_money;\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    zt(ztt) {\n      if (ztt == 0) {\n        return \"待审核\";\n      }\n      if (ztt == 1) {\n        return \"驳回\";\n      }\n      if (ztt == 2) {\n        return \"通过\";\n      }\n    },\n    //时间格式化\n    filterTime(timestamp) {\n      return formatDate(timestamp);\n    },\n    handleClick(tab, event) {\n      console.log(tab, event);\n    }\n  },\n  mounted() {\n    // this.findrk();\n    this.findId();\n    this.find();\n  }\n};","map":{"version":3,"names":["$http","formatDate","date","time","Date","console","log","year","getFullYear","month","getMonth","toString","padStart","day","getDate","data","activeIndex","zdr","djzt","gys","date1","lxr","fs","th","jhr","thyy","bz","buy_code","tableData","tableData1","tableData2","multipleSelection","yrksl","drksl","hpsl","zje","rksl","rkje","activeName","thsl","zje1","thje","methods","delAll","$router","push","path","query","sr_code","$route","sgb","go","gb","fzcg","$message","message","type","handleSelect","key","keyPath","handleSelectionChange","val","findId","get","then","res","create_by","create_time","sale_state","cust_code","sr_date","link_man","link_way","sale_duty_man","remark","remarks","catch","err","find","sale_code","FindNUm","i","map","item","return_num","return_money","length","zt","ztt","filterTime","timestamp","handleClick","tab","event","mounted"],"sources":["src/components/SalesReturnsCode.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div id=\"addsss\">\r\n      <h3 style=\"margin-left: 35px; padding-top: 30px; display: inline-block\">\r\n        销售退货单号：{{ this.$route.query.sr_code }}\r\n        <img\r\n          src=\"../Images/fz.png\"\r\n          width=\"20px\"\r\n          height=\"20px\"\r\n          style=\"vertical-align: middle; margin-left: 120px\"\r\n          @click=\"fzcg\"\r\n        />\r\n      </h3>\r\n\r\n      <div style=\"float: right; padding-top: 30px; margin-right: 35px\">\r\n        <el-button id=\"tex3\" @click=\"inserok()\" icon=\"el-icon-plus\"\r\n          >入库</el-button\r\n        >\r\n        <!-- <el-button  id=\"tex4\"  icon=\"el-icon-edit\">修改</el-button> -->\r\n        <el-button id=\"tex5\" @click=\"delAll()\" v-if=\"th\" icon=\"el-icon-minus\"\r\n          >退货</el-button\r\n        >\r\n        <el-button id=\"tex6\" icon=\"el-icon-close\" @click=\"sgb\">关闭</el-button>\r\n      </div>\r\n\r\n      <table\r\n        style=\"\r\n          width: 1100px;\r\n          line-height: 55px;\r\n          margin-left: 35px;\r\n          padding-top: 20px;\r\n        \"\r\n      >\r\n        <tr>\r\n          <td>\r\n            制单人<i style=\"margin-left: 50px; color: #999999\">{{ zdr }}</i>\r\n          </td>\r\n          <!-- <td>所在部门<i style=\"margin-left: 50px\">123</i></td> -->\r\n          <td>\r\n            制单时间<i style=\"margin-left: 50px; color: #999999\">{{\r\n              filterTime(date)\r\n            }}</i>\r\n          </td>\r\n          <td>\r\n            销售负责人<i style=\"margin-left: 65px; color: #999999\">{{ jhr }}</i>\r\n          </td>\r\n\r\n          <td>\r\n            单据状态<i style=\"margin-left: 50px; font-weight: bold\">{{\r\n              zt(djzt)\r\n            }}</i>\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>\r\n            客户名称<i style=\"margin-left: 50px; color: #999999\">{{ gys }}</i>\r\n          </td>\r\n          <td>\r\n            退货日期<i style=\"margin-left: 50px; color: #999999\">{{\r\n              filterTime(date1)\r\n            }}</i>\r\n          </td>\r\n          <!-- <td>进货部门<i style=\"margin-left: 50px\">123</i></td> -->\r\n          <td>\r\n            联系方式<i style=\"margin-left: 50px; color: #999999\">{{ fs }}</i>\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>\r\n            联系人<i style=\"margin-left: 50px; color: #999999\">{{ lxr }}</i>\r\n          </td>\r\n          <td>\r\n            退货原因<i style=\"margin-left: 50px; color: #999999\">{{ thyy }}</i>\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>\r\n            备注<i style=\"margin-left: 65px; color: #999999\">{{ bz }}</i>\r\n          </td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n    <div id=\"addxx\">\r\n      <el-tabs id=\"menu\" v-model=\"activeName\" @tab-click=\"handleClick\">\r\n        <el-tab-pane label=\"退货明细\" style=\"margin-top: -10px\" name=\"first\">\r\n          <el-button id=\"tex9\" icon=\"el-icon-download\">导出</el-button>\r\n          <el-table\r\n            tooltip-effect=\"dark\"\r\n            @selection-change=\"handleSelectionChange\"\r\n            :data=\"tableData\"\r\n            :header-cell-style=\"{\r\n              background: '#F8F8F9',\r\n              color: '#606266',\r\n              'text-align': 'center',\r\n            }\"\r\n            :cell-style=\"{ 'text-align': 'center' }\"\r\n            id=\"jhmx\"\r\n          >\r\n            <el-table-column type=\"selection\" width=\"55\"> </el-table-column>\r\n            <el-table-column label=\"序号\" type=\"index\" width=\"50\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"item_name\" label=\"货品名称\" width=\"110\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"item_code\" label=\"货品编号\" width=\"110\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"item_type_name\" label=\"货品类型\" width=\"110\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"model_code\" label=\"规格型号\" width=\"110\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"unit\" label=\"单位\" width=\"110\">\r\n            </el-table-column>\r\n\r\n            <el-table-column prop=\"inbill3_num\" label=\"销售数量\" width=\"110\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"treat_num\" label=\"退货数量\" width=\"114\">\r\n              <template slot-scope=\"scope\">\r\n                <input\r\n                  readonly\r\n                  style=\"border: 0px solid; margin-left: 30px\"\r\n                  v-model=\"tableData[scope.$index].return_num\"\r\n                />\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"sale_price\" label=\"销售单价\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column label=\"退货金额\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                <input\r\n                  v-model=\"tableData[scope.$index].return_money\"\r\n                  style=\"border: 0px solid\"\r\n                  readonly\r\n                />\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n          <div id=\"hj\">\r\n            <span style=\"line-height: 45px; font-size: 12px; margin-left: 25px\"\r\n              >合计</span\r\n            >\r\n            <p style=\"float: right\">\r\n              <span style=\"line-height: 45px; font-size: 12px\">退货数量：</span>\r\n              <span\r\n                style=\"\r\n                  line-height: 45px;\r\n                  font-weight: bold;\r\n                  color: red;\r\n                  font-size: 12px;\r\n                  margin-right: 35px;\r\n                \"\r\n                >{{ thsl }}</span\r\n              >\r\n              <span style=\"line-height: 45px; font-size: 12px\">退货金额：</span>\r\n              <span\r\n                style=\"\r\n                  line-height: 45px;\r\n                  font-weight: bold;\r\n                  color: red;\r\n                  font-size: 12px;\r\n                  margin-right: 35px;\r\n                \"\r\n                >{{ thje }}</span\r\n              >\r\n            </p>\r\n          </div></el-tab-pane\r\n        >\r\n        <el-tab-pane label=\"出库记录\" style=\"margin-top: -10px\" name=\"second\">\r\n          <el-button id=\"tex9\" icon=\"el-icon-download\">导出</el-button>\r\n          <el-table\r\n            tooltip-effect=\"dark\"\r\n            @selection-change=\"handleSelectionChange\"\r\n            :data=\"tableData1\"\r\n            :header-cell-style=\"{\r\n              background: '#F8F8F9',\r\n              color: '#606266',\r\n              'text-align': 'center',\r\n            }\"\r\n            :cell-style=\"{ 'text-align': 'center' }\"\r\n            id=\"jhmx\"\r\n          >\r\n            <el-table-column type=\"selection\" width=\"55\"> </el-table-column>\r\n            <el-table-column label=\"序号\" type=\"index\" width=\"50\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"inbill_bh\" label=\"入库单号\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column label=\"入库日期\" width=\"130\">\r\n              <template slot-scope=\"scope\">\r\n                <span>{{ filterTime(scope.row.inbll_date) }}</span>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"ware_name\" label=\"仓库名称\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"names\"\r\n              :show-overflow-tooltip=\"true\"\r\n              label=\"入库货品\"\r\n              width=\"120px\"\r\n            >\r\n            </el-table-column>\r\n            <el-table-column prop=\"total_buy_num\" label=\"入库数量\" width=\"94\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"total_price\" label=\"入库金额\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"inbill_duty_man\" label=\"经办人\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"create_by\" label=\"制单人\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column label=\"制单时间\" width=\"130\">\r\n              <template slot-scope=\"scope\">\r\n                <span>{{ filterTime(scope.row.create_time) }}</span>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n          <div id=\"hj\">\r\n            <span style=\"line-height: 45px; font-size: 12px; margin-left: 25px\"\r\n              >合计</span\r\n            >\r\n            <p style=\"float: right\">\r\n              <span style=\"line-height: 45px; font-size: 12px\">入库数量：</span>\r\n              <span\r\n                style=\"\r\n                  line-height: 45px;\r\n                  font-weight: bold;\r\n                  color: red;\r\n                  font-size: 12px;\r\n                  margin-right: 35px;\r\n                \"\r\n                id=\"sumNumber\"\r\n                >{{ rksl }}</span\r\n              >\r\n              <span style=\"line-height: 45px; font-size: 12px\">入库金额：</span>\r\n              <span\r\n                style=\"\r\n                  line-height: 45px;\r\n                  font-weight: bold;\r\n                  color: red;\r\n                  font-size: 12px;\r\n                  margin-right: 30px;\r\n                \"\r\n                id=\"sumSalary\"\r\n                >{{ rkje }}￥</span\r\n              >\r\n            </p>\r\n          </div>\r\n        </el-tab-pane>\r\n      </el-tabs>\r\n    </div>\r\n    <div id=\"notification-container\">\r\n      <div id=\"dibu\">\r\n        <el-button id=\"bcccc\" @click=\"gb()\">关闭</el-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport $http from \"@/http/base\";\r\nexport function formatDate(date) {\r\n  let time = new Date(date);\r\n  console.log(time);\r\n  let year = time.getFullYear();\r\n  console.log(year);\r\n  let month = time.getMonth() + 1;\r\n  month = month.toString().padStart(2, \"0\");\r\n  let day = time.getDate().toString().padStart(2, \"0\");\r\n  return year + \"-\" + month + \"-\" + day + \"\";\r\n}\r\nexport default {\r\n  data() {\r\n    return {\r\n      activeIndex: \"1\",\r\n      zdr: \"\",\r\n      date: \"\",\r\n      djzt: \"\",\r\n      gys: \"\",\r\n      date1: \"\",\r\n      lxr: \"\",\r\n      fs: \"\",\r\n      th: false,\r\n      jhr: \"\",\r\n      thyy: \"\",\r\n      bz: \"\",\r\n      buy_code: \"\",\r\n      tableData: [],\r\n      tableData1: [],\r\n      tableData2: [],\r\n      multipleSelection: [],\r\n      yrksl: 0,\r\n      drksl: 0,\r\n      hpsl: 0,\r\n      zje: 0,\r\n\r\n      rksl: 0,\r\n      rkje: 0,\r\n      activeName: \"first\",\r\n\r\n      thsl: 0,\r\n      zje1: 0,\r\n      thje: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    delAll() {\r\n      this.$router.push({\r\n        path: \"/SalesReturnsTH\",\r\n        query: {\r\n          sr_code: this.$route.query.sr_code,\r\n        },\r\n      });\r\n    },\r\n    sgb() {\r\n      this.$router.go(-1);\r\n    },\r\n    //关闭\r\n    gb() {\r\n      this.$router.go(-1);\r\n    },\r\n    fzcg() {\r