{"ast":null,"code":"import $http from '../http/base.js';\nexport default {\n  data() {\n    return {\n      str: \"1=1\",\n      dialogVisible: true,\n      dialogFormVisible: false,\n      gridData: [],\n      multipleSelection: [],\n      // 选中的行数据\n      currentPage: 1,\n      //当前页\n      pagesize: 4,\n      //每页显示数\n      currentPage1: 1,\n      //当前页\n      pagesize1: 4,\n      //每页显示数\n      rows: 0,\n      //总行数\n      rows1: 0,\n      tableData: [],\n      formtable: {\n        dh: \"\",\n        zt: \"\",\n        gys: \"\",\n        jhr: \"\",\n        data: \"\"\n      },\n      xx: {\n        wei: 0,\n        cw: \"\",\n        sl: \"\",\n        bei: \"\"\n      },\n      countryList: [],\n      countryList1: [],\n      countryList2: [],\n      formInline: {\n        dh: '',\n        lx: '',\n        mc: '',\n        date: '',\n        bei: \"\",\n        lxr: \"\",\n        fs: \"\",\n        gldh: \"\",\n        gys: \"\",\n        jin: \"\"\n      },\n      rules: {\n        startDate: [{\n          required: true,\n          message: \"请选择入库时间!\",\n          trigger: \"blur\"\n        }],\n        dh: [{\n          required: true,\n          message: \"请填写单号!\",\n          trigger: \"blur\"\n        }],\n        lx: [{\n          required: true,\n          message: \"请选择入库类型!\",\n          trigger: \"change\"\n        }],\n        mc: [{\n          required: true,\n          message: \"请选择仓库名称!\",\n          trigger: \"change\"\n        }]\n      },\n      rules1: {\n        link_man: [{\n          required: true,\n          message: \"请选择入库时间!\",\n          trigger: \"blur\"\n        }]\n      }\n    };\n  },\n  methods: {\n    cang() {\n      var t = this;\n      $http.post(\"http://localhost:5149/api/Wh_inbill_h/Bs_ware_areaName?bh=\" + this.formInline.mc).then(res => {\n        t.countryList1 = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    zt(i) {\n      if (i == 0) return \"草稿\";\n      if (i == 1) return \"待审核\";\n      if (i == 2) return \"驳回\";\n      if (i == 3) return \"已完成\";\n    },\n    //查询方法\n    find() {\n      var t = this;\n      $http.post(\"http://localhost:5149/api/Wh_inbill_h/FenOrder?page=\" + t.pagesize + \"&pageSize=\" + t.currentPage + \"&str=\" + t.str).then(res => {\n        t.gridData = res.list;\n        t.rows1 = res.rows;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //进货查询\n    onSubmit() {\n      this.str = \"1=1 and is_del=0 \";\n      if (this.formtable.dh != \"\") {\n        this.str += \"and buy_code like '%\" + this.formtable.dh + \"%'\";\n      }\n      if (this.formtable.zt != \"\") {\n        this.str += \"and sale_state like '%\" + this.formtable.zt + \"%'\";\n      }\n      if (this.formtable.gys != \"\") {\n        this.str += \"and supply_code like '%\" + this.formtable.gys + \"%'\";\n      }\n      if (this.formtable.data != \"\") {\n        this.str += \"and buy_date = '\" + this.formtable.data + \"'\";\n      }\n      if (this.formtable.jhr != \"\") {\n        this.str += \"and buy_man = '\" + this.formtable.jhr + \"'\";\n      }\n      alert(this.str);\n      this.find();\n    },\n    //下拉框\n    CK() {\n      var t = this;\n      $http.get(\"http://localhost:5149/api/Bs_warehouse/Bs_warehouseSelect\").then(res => {\n        t.countryList = res;\n      }).catch(err => {\n        alert(err);\n      });\n      $http.get(\"http://localhost:5149/api/Wh_inbill_h/FindUser\").then(res => {\n        t.countryList1 = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //选择单号\n    xz() {\n      this.dialogFormVisible = true;\n      $http.get(\"http://localhost:5149/api/Wh_inbill_h/FindUser\").then(res => {\n        this.countryList2 = res;\n      }).catch(err => {\n        alert(err);\n      });\n      this.find();\n    },\n    filterTime(timestamp) {\n      return formatDate(timestamp);\n    },\n    //保存\n    bc() {},\n    //关闭\n    gb() {\n      //this.$router.push('/Wh_inbill_h');\n      this.$router.go(-1);\n    },\n    //删除\n    sc() {\n      alert(\"123\");\n    },\n    handleRowClick(i) {\n      if (i.sale_state != 2) {\n        alert(\"请选择已完成的订单！\");\n        return;\n      }\n      this.formInline.gldh = i.buy_code;\n      this.formInline.gys = i.supply_code;\n      this.formInline.lxr = i.link_man;\n      this.formInline.fs = i.link_way;\n      this.dialogFormVisible = false;\n      var t = this;\n      $http.post(\" http://localhost:5149/api/Wh_inbill_h/Wh_orderbuy_dBH?bh=\" + i.buy_code).then(res => {\n        t.tableData = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //每页显示数改变事件\n    handleSizeChange(size) {\n      this.pagesize = size;\n      this.currentPage = 1;\n      // this.find();\n    },\n\n    //页码改变事件\n    handleCurrentChange(page) {\n      this.currentPage = page;\n      // this.find();\n    },\n\n    //每页显示数改变事件\n    handleSizeChange1(size) {\n      this.pagesize1 = size;\n      this.currentPage1 = 1;\n\n      // this.find();\n    },\n\n    //页码改变事件\n    handleCurrentChange1(page) {\n      this.currentPage1 = page;\n      // this.find();\n    },\n\n    //重置\n    cz() {\n      this.formtable = {\n        dh: \"\",\n        zt: \"\",\n        gys: \"\",\n        jhr: \"\",\n        data: \"\"\n      };\n    }\n  },\n  mounted() {\n    this.CK();\n  }\n};\nexport function formatDate(date) {\n  let time = new Date(date);\n  let year = time.getFullYear();\n  let month = time.getMonth() + 1;\n  month = month.toString().padStart(2, \"0\");\n  let day = time.getDate().toString().padStart(2, \"0\");\n  return year + '年' + month + '月' + day + '日';\n}","map":{"version":3,"names":["$http","data","str","dialogVisible","dialogFormVisible","gridData","multipleSelection","currentPage","pagesize","currentPage1","pagesize1","rows","rows1","tableData","formtable","dh","zt","gys","jhr","xx","wei","cw","sl","bei","countryList","countryList1","countryList2","formInline","lx","mc","date","lxr","fs","gldh","jin","rules","startDate","required","message","trigger","rules1","link_man","methods","cang","t","post","then","res","catch","err","alert","i","find","list","onSubmit","CK","get","xz","filterTime","timestamp","formatDate","bc","gb","$router","go","sc","handleRowClick","sale_state","buy_code","supply_code","link_way","handleSizeChange","size","handleCurrentChange","page","handleSizeChange1","handleCurrentChange1","cz","mounted","time","Date","year","getFullYear","month","getMonth","toString","padStart","day","getDate"],"sources":["src/components/AddWh_inbill_h.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <div id=\"adds\">\r\n            <div style=\"height: 60;line-height: 60px; box-shadow: 0px -1px 7px rgba(0, 0, 0, 0.2);\r\n    background-color: #fff;\">\r\n                <h4 style=\"margin-left: 25px;\">基础信息</h4>\r\n            </div>\r\n            <div style=\"margin-top: 30px;\">\r\n                <el-form :inline=\"true\" :model=\"formInline\" :rules=\"rules\" class=\"demo-form-inline\">\r\n                    <el-form-item label=\"入库单号\" id=\"yy\" prop=\"dh\">\r\n                        <el-input v-model=\"formInline.dh\" id=\"tex\" placeholder=\"自动获取系统编号\"  :readonly=\"true\"></el-input>\r\n                    </el-form-item>\r\n                    \r\n                         <el-form-item label=\"关联单号\" id=\"yyy\">\r\n                               <el-input v-model=\"formInline.gldh\" id=\"tex\" placeholder=\"请输入\" @focus=\"xz()\"></el-input>\r\n                              \r\n                            </el-form-item>\r\n                           \r\n                    <el-form-item label=\"供应商\" id=\"yyy\">\r\n                        <el-input v-model=\"formInline.gys\" id=\"tex\" placeholder=\"请输入\" :disabled=\"true\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"入库类型\"   id=\"yy\" prop=\"lx\">\r\n                                    <el-select v-model=\"formInline.lx\" id=\"tex\" clearable filterable placeholder=\"请选择\">\r\n                                        <el-option label=\"进货入库\" value=\"进货入库\"></el-option>\r\n                                        <el-option label=\"生产入库\" value=\"生产入库\"></el-option>\r\n                                         <el-option label=\"退货入库\" value=\"退货入库\"></el-option>\r\n                                          <el-option label=\"换货入库\" value=\"换货入库\"></el-option>\r\n                                    </el-select>\r\n                                </el-form-item>\r\n                    <el-form-item label=\"进货日期\" id=\"yyy\" >\r\n                         <el-date-picker v-model=\"formInline.jin\" id=\"tex\" type=\"datetime\" placeholder=\"选择日期时间\" value-format=\"yyyy-MM-dd HH:mm:ss\"></el-date-picker>\r\n                      \r\n                    </el-form-item>  \r\n                    <el-form-item label=\"联系人\" id=\"yy1\">\r\n                        <el-input v-model=\"formInline.lxr\" id=\"tex\" placeholder=\"请输入\" :disabled=\"true\"></el-input>\r\n                    </el-form-item>\r\n                    \r\n                     <el-form-item label=\"仓库名称\" id=\"yy\"  prop=\"mc\" >\r\n                                        <el-select v-model=\"formInline.mc\" clearable filterable id=\"tex\" placeholder=\"请选择\" @change=\"cang()\" >\r\n                                            <el-option\r\n                                        v-for=\"item in countryList\"\r\n                                        :key=\"item.warehouse_id\"\r\n                                        :label=\"item.ware_name\"\r\n                                        :value=\"item.warehouse_id\"\r\n                                       \r\n                                      ></el-option>\r\n                                        </el-select>\r\n                                    </el-form-item>\r\n                                    <el-form-item label=\"联系方式\" id=\"yyy\">\r\n                            <el-input v-model=\"formInline.fs\" id=\"tex\" placeholder=\"请输入\" :disabled=\"true\"></el-input>\r\n                        </el-form-item>\r\n                     <el-form-item label=\"经办人\" id=\"yyy\">\r\n                            <el-select v-model=\"formInline.jbr\" id=\"tex\" clearable filterable placeholder=\"请选择\">\r\n                               <el-option\r\n                                            v-for=\"item in countryList1\"\r\n                                            :key=\"item.u_id\"\r\n                                            :label=\"item.u_name\"\r\n                                            :value=\"item.u_name\"\r\n                                          ></el-option>\r\n\r\n                            </el-select>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"入库日期\" prop=\"startDate\" id=\"ru\">\r\n                                     <el-date-picker v-model=\"formInline.date\" id=\"tex\" type=\"datetime\" placeholder=\"选择日期时间\" value-format=\"yyyy-MM-dd HH:mm:ss\">\r\n                    </el-date-picker>\r\n                                </el-form-item>\r\n                    <el-form-item label=\"备注\" id=\"yyyy\">\r\n                        <el-input id=\"te\" type=\"textarea\" placeholder=\"请输入\" v-model=\"formInline.bei\" maxlength=\"100\" :rows=\"5\"\r\n                            show-word-limit>\r\n                        </el-input>\r\n                    </el-form-item>\r\n                </el-form>\r\n            </div>\r\n        </div>\r\n        <div id=\"addx\">\r\n            <div style=\"height: 60;line-height: 60px; box-shadow: 0px -1px 7px rgba(0, 0, 0, 0.2);\r\n    background-color: #fff;\">\r\n                <h4 style=\"margin-left: 25px;\">入库明细</h4>\r\n            </div>\r\n            <div style=\"margin-top: 50px;margin-left: 30px;\">\r\n\r\n                <el-table tooltip-effect=\"dark\"  :data=\"tableData\" \r\n                    :header-cell-style=\"{ background: '#F8F8F9', color: '#606266', 'text-align': 'center' }\" :cell-style=\"{ 'text-align': 'center' }\"\r\n     id=\"ttt\">\r\n                    <el-table-column\r\n                            label=\"序号\"\r\n                            fixed\r\n              type=\"index\"\r\n              width=\"50\">\r\n            </el-table-column>\r\n\r\n                    <el-table-column fixed prop=\"item_name\" label=\"货品名称\" width=\"100\" >\r\n                    </el-table-column>\r\n                    <el-table-column  fixed prop=\"item_code\" label=\"货品编号\" width=\"100\">\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"item_type_name\" label=\"货品类型\" width=\"80\" >\r\n                        </el-table-column>\r\n                    <el-table-column prop=\"model_code\" label=\"规格型号\" width=\"80\" >\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"unit\" label=\"单位\" width=\"80\" >\r\n                        </el-table-column>\r\n                       \r\n                        <el-table-column prop=\"buy_num\" label=\"进货数量\" width=\"80\" >\r\n                        </el-table-column>\r\n                        <el-table-column prop=\"inbill_num\" v-model=\"xx.wei\" label=\"已入库数量\" width=\"140\" >\r\n                            </el-table-column>\r\n                            <el-table-column prop=\"buy_num\" label=\"未入库数量\" width=\"140\" >\r\n                            </el-table-column>\r\n                    <el-table-column  label=\"入库仓位\" width=\"140\" >\r\n                        <el-select v-model=\"xx.cw\"  placeholder=\"请选择\">\r\n                                                <el-option\r\n                                            v-for=\"item in countryList1\"\r\n                                            :key=\"item.ware_area_id\"\r\n                                            :label=\"item.area_name\"\r\n                                            :value=\"item.area_name\"\r\n                                          ></el-option>\r\n                                            </el-select>\r\n                    </el-table-column>\r\n                    \r\n                    <el-table-column  label=\"本次入库数量\" width=\"140\" >\r\n                        <el-input v-model=\"xx.sl\" id=\"bb\" ></el-input>\r\n                    </el-table-column>\r\n                     <el-table-column prop=\"inbill_price\" label=\"进货单价\" width=\"100\" >\r\n                            </el-table-column>\r\n                            <el-table-column  label=\"进货金额\" width=\"100\" >\r\n                            </el-table-column>\r\n\r\n                    <el-table-column  label=\"备注\" width=\"300\">\r\n                         <el-input v-model=\"xx.bei\"  style=\"width: 300px;\"></el-input>\r\n                    </el-table-column>\r\n                   \r\n                </el-table>\r\n                <!-- 分页 -->\r\n                <el-pagination id=\"fenye1\" @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\"\r\n                    :current-page=\"currentPage\" :page-sizes=\"[4, 8, 12, 16]\" :page-size=\"pagesize\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\" :total=\"rows\">\r\n                </el-pagination>\r\n            </div>\r\n        </div>\r\n    <!-- 选择进货单 -->\r\n                <el-dialog title=\"选择进货单\" :visible.sync=\"dialogFormVisible\" width=\"800px\" id=\"tjsh\">\r\n\r\n        <el-form :inline=\"true\" :model=\"formtable\" class=\"demo-form-inline\">\r\n          <el-form-item label=\"\" style=\"width:221px\">\r\n            <el-input v-model=\"formtable.dh\"  id=\"bddd\" placeholder=\"请输入进货单号\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"\"  style=\"margin-left: 30px; \">\r\n            <el-select v-model=\"formtable.zt\" clearable filterable placeholder=\"请选择单据状态\" id=\"bddd\">\r\n           \r\n                                    <el-option label=\"待审核\" value=\"0\"></el-option>\r\n                                    <el-option label=\"驳回\" value=\"1\"></el-option>\r\n                                    <el-option label=\"已完成\" value=\"2\"></el-option>\r\n                                   \r\n            </el-select>\r\n          </el-form-item>\r\n          \r\n              <el-form-item label=\"\"  style=\"margin-left: 30px; \">\r\n                <el-select v-model=\"formtable.gys\" clearable filterable placeholder=\"请选择供应商\" id=\"bddd\">\r\n                  <el-option label=\"区域一\" value=\"shanghai\"></el-option>\r\n                  <el-option label=\"区域二\" value=\"beijing\"></el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n              <el-form-item label=\"\"  style=\"margin-left: 0px; \">\r\n                <el-select v-model=\"formtable.jhr\" clearable filterable placeholder=\"请选择进货人\" id=\"bddd\">\r\n                  <el-option\r\n                                                v-for=\"item in countryList2\"\r\n                                                :key=\"item.u_id\"\r\n                                                :label=\"item.u_name\"\r\n                                                :value=\"item.u_name\"\r\n                                              ></el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n              <el-form-item label=\"\"  style=\"margin-left: 30px; \">\r\n                 <el-date-picker v-model=\"formtable.date\" id=\"bddd\" type=\"datetime\" placeholder=\"请选择进货日期\" value-format=\"yyyy-MM-dd HH:mm:ss\"></el-date-picker>\r\n              </el-form-item>\r\n          <el-form-item>\r\n            <el-button  icon=\"el-icon-refresh\" style=\"margin-left: 60px;\"  id=\"bddddd\" @click=\"cz()\" >重置</el-button>\r\n            <el-button type=\"primary\" icon=\"el-icon-search\" style=\"margin-left:20px;\" @click=\"onSubmit()\"  id=\"bdddd\" >查询</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n\r\n                \r\n                    <el-table :data=\"gridData\" :header-cell-style=\"{ background: '#F8F8F9', color: '#606266' }\"  @row-click=\"handleRowClick\">\r\n            <!-- <el-table-column type=\"selection\"  width=\"55\">\r\n                        </el-table-column> -->\r\n                       \r\n\r\n                        <el-table-column\r\n                        label=\"序号\"\r\n          type=\"index\"\r\n          width=\"50\"\r\n          fixed>\r\n        </el-table-column>\r\n            <el-table-column fixed property=\"buy_code\" label=\"进货单号\" width=\"100\"></el-table-column>\r\n            <el-table-column  label=\"单据状态\" width=\"120\">\r\n                <!-- <template slot-scope='scope'>\r\n                                            <span>{{ zt(scope.row.sale_state) }}</span>\r\n                                      </template> -->\r\n                                       <template slot-scope=\"scope\">\r\n                                <span v-if=\"scope.row.sale_state == 0\" style=\"font-weight: 600;font-size: 13px;\">待审核</span>\r\n                                <span v-if=\"scope.row.sale_state == 1\"\r\n                                    style=\"color: blue;font-weight: 600;font-size: 13px;\">驳回</span>\r\n                                <span v-if=\"scope.row.sale_state == 2\"\r\n                                    style=\"color: red;font-weight: 600;font-size: 13px;\">已完成</span>\r\n                            </template>\r\n            </el-table-column>\r\n            <el-table-column  label=\"进货日期\" width=\"140\">\r\n                 <template slot-scope='scope'>\r\n                                        <span>{{ filterTime(scope.row.buy_date) }}</span>\r\n                                  </template>\r\n            </el-table-column>\r\n            <el-table-column property=\"supply_code\" label=\"供应商\" width=\"100\"></el-table-column>\r\n            <el-table-column property=\"buy_man\" label=\"进货人\" width=\"1