nce_name,\n            tax_no: t.formInline1.tax_no,\n            bank_no: t.formInline1.bank_no,\n            open_bank: t.formInline1.open_bank,\n            open_adr: t.formInline1.open_adr,\n            finance_type: \"\"\n          }).then(res => {\n            console.log(res);\n            if (res) {\n              this.$router.go(-1);\n              this.$message({\n                message: \"修改成功！\",\n                type: \"success\"\n              });\n            }\n          }).catch(err => {\n            console.log(err);\n          });\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    //关闭\n    gb() {\n      this.$router.go(-1);\n    },\n    fzr() {\n      $http.post(\"http://localhost:5149/api/SupplyView/Czr\").then(res => {\n        this.List = res;\n        console.log(res);\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    fl() {\n      $http.post(\"http://localhost:5149/api/SupplyView/FL\").then(res => {\n        this.List2 = res;\n        console.log(res);\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    hy() {\n      $http.post(\"http://localhost:5149/api/SupplyView/HY\").then(res => {\n        this.List1 = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    fill() {\n      var id = this.$route.query.id;\n      $http.get(\"http://localhost:5149/api/SupplyView/SelectID?id=\" + id).then(res => {\n        this.formInline.supply_code = res.supply_code;\n        this.formInline.supply_name = res.supply_name;\n        this.formInline.line_type = res.line_type;\n        this.formInline.supply_type_code = res.supply_type_code;\n        this.formInline.link_man = res.link_man;\n        this.formInline.link_tel = res.link_tel;\n        this.formInline.e_mail = res.e_mail;\n        this.formInline.stock_duty_man = res.stock_duty_man;\n        this.radio = res.inbill_state;\n        this.formInline.supply_adr = res.supply_adr;\n        this.formInline