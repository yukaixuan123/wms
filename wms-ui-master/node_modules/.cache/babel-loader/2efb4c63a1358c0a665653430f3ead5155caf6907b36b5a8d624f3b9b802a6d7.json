{"ast":null,"code":"import $http from '../http/base.js';\nexport default {\n  data() {\n    return {\n      dialogVisible: true,\n      dialogFormVisible: false,\n      gridData: [],\n      multipleSelection: [],\n      // 选中的行数据\n      currentPage: 1,\n      //当前页\n      pagesize: 4,\n      //每页显示数\n      currentPage1: 1,\n      //当前页\n      pagesize1: 4,\n      //每页显示数\n      rows: 0,\n      //总行数\n      rows1: 0,\n      tableData: [{\n        date: '2016-05-03',\n        name: '王小虎',\n        province: '上海',\n        city: '普陀区',\n        address: '上海市普陀区金沙江路 1518 弄',\n        zip: 200333\n      }, {\n        date: '2016-05-02',\n        name: '王小虎',\n        province: '上海',\n        city: '普陀区',\n        address: '上海市普陀区金沙江路 1518 弄',\n        zip: 200333\n      }, {\n        date: '2016-05-04',\n        name: '王小虎',\n        province: '上海',\n        city: '普陀区',\n        address: '上海市普陀区金沙江路 1518 弄',\n        zip: 200333\n      }, {\n        date: '2016-05-01',\n        name: '王小虎',\n        province: '上海',\n        city: '普陀区',\n        address: '上海市普陀区金沙江路 1518 弄',\n        zip: 200333\n      }],\n      formtable: {\n        dh: \"\",\n        zt: \"\",\n        gys: \"\",\n        jhr: \"\",\n        data: \"\"\n      },\n      countryList: [],\n      formInline: {\n        dh: '',\n        lx: '',\n        mc: '',\n        date: '',\n        bei: \"\",\n        lxr: \"\",\n        fs: \"\",\n        gldh: \"\",\n        gys: \"\",\n        jin: \"\"\n      },\n      rules: {\n        startDate: [{\n          required: true,\n          message: \"请选择入库时间!\",\n          trigger: \"blur\"\n        }],\n        dh: [{\n          required: true,\n          message: \"请填写单号!\",\n          trigger: \"blur\"\n        }],\n        lx: [{\n          required: true,\n          message: \"请选择入库类型!\",\n          trigger: \"change\"\n        }],\n        mc: [{\n          required: true,\n          message: \"请选择仓库名称!\",\n          trigger: \"change\"\n        }]\n      },\n      str: \"1=1\"\n    };\n  },\n  methods: {\n    zt(i) {\n      if (i == 0) return \"草稿\";\n      if (i == 1) return \"待审核\";\n      if (i == 2) return \"驳回\";\n      if (i == 3) return \"已完成\";\n    },\n    //查询方法\n    find() {\n      var t = this;\n      $http.post(\"http://localhost:5149/api/Wh_inbill_h/FenOrder?page=\" + t.pagesize + \"&pageSize=\" + t.currentPage + \"&str=\" + t.str).then(res => {\n        console.log(res);\n        t.gridData = res.list;\n        t.rows1 = res.rows;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //进货查询\n    onSubmit() {\n      str = \"1=1 and is_del=0 \";\n      if (this.formInline.dh != \"\") {\n        str += \"and buy_code like '%\" + this.formInline.bh + \"%'\";\n      }\n      if (this.formInline.zt != \"\") {\n        str += \"and sale_state like '%\" + this.formInline.zt + \"%'\";\n      }\n      if (this.formInline.gys != \"\") {\n        str += \"and supply_code like '%\" + this.formInline.gys + \"%'\";\n      }\n      if (this.formInline.date != \"\") {\n        str += \"and buy_date = '\" + this.formInline.date + \"'\";\n      }\n      if (this.formInline.jhr != \"\") {\n        str += \"and buy_man = '\" + this.formInline.date + \"'\";\n      }\n      this.find();\n    },\n    //仓库下拉框\n    CK() {\n      var t = this;\n      $http.get(\"http://localhost:5149/api/Bs_warehouse/Bs_warehouseSelect\").then(res => {\n        t.countryList = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //选择单号\n    xz() {\n      this.dialogFormVisible = true;\n      this.find();\n    },\n    filterTime(timestamp) {\n      return formatDate(timestamp);\n    },\n    //保存\n    bc() {},\n    //关闭\n    gb() {\n      //this.$router.push('/Wh_inbill_h');\n      this.$router.go(-1);\n    },\n    //删除\n    sc() {\n      alert(\"123\");\n    },\n    //选择\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n    handleSelectionChange1() {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n    //每页显示数改变事件\n    handleSizeChange(size) {\n      this.pagesize = size;\n      this.currentPage = 1;\n      // this.find();\n    },\n\n    //页码改变事件\n    handleCurrentChange(page) {\n      this.currentPage = page;\n      // this.find();\n    },\n\n    //每页显示数改变事件\n    handleSizeChange1(size) {\n      this.pagesize1 = size;\n      this.currentPage1 = 1;\n\n      // this.find();\n    },\n\n    //页码改变事件\n    handleCurrentChange1(page) {\n      this.currentPage1 = page;\n      // this.find();\n    },\n\n    //重置\n    cz() {\n      this.formtable = {\n        dh: \"\",\n        zt: \"\",\n        gys: \"\",\n        jhr: \"\",\n        data: \"\"\n      };\n    }\n  },\n  mounted() {\n    this.CK();\n  }\n};\nexport function formatDate(date) {\n  let time = new Date(date);\n  let year = time.getFullYear();\n  let month = time.getMonth() + 1;\n  month = month.toString().padStart(2, \"0\");\n  let day = time.getDate().toString().padStart(2, \"0\");\n  return year + '年' + month + '月' + day + '日';\n}","map":{"version":3,"names":["$http","data","dialogVisible","dialogFormVisible","gridData","multipleSelection","currentPage","pagesize","currentPage1","pagesize1","rows","rows1","tableData","date","name","province","city","address","zip","formtable","dh","zt","gys","jhr","countryList","formInline","lx","mc","bei","lxr","fs","gldh","jin","rules","startDate","required","message","trigger","str","methods","i","find","t","post","then","res","console","log","list","catch","err","alert","onSubmit","bh","CK","get","xz","filterTime","timestamp","formatDate","bc","gb","$router","go","sc","handleSelectionChange","val","handleSelectionChange1","handleSizeChange","size","handleCurrentChange","page","handleSizeChange1","handleCurrentChange1","cz","mounted","time","Date","year","getFullYear","month","getMonth","toString","padStart","day","getDate"],"sources":["src/components/AddWh_inbill