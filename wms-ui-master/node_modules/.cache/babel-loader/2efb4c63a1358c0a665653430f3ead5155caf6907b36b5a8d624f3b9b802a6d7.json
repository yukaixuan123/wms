orm-item label=\"进货日期\" id=\"yyy\" >\r\n                        <el-input v-model=\"formInline.jin\" id=\"tex\" placeholder=\"请输入\"></el-input>\r\n                    </el-form-item>\r\n                    \r\n                            \r\n                       \r\n                    <el-form-item label=\"联系人\" id=\"yyy\">\r\n                        <el-input v-model=\"formInline.lxr\" id=\"tex\" placeholder=\"请输入\" :disabled=\"true\"></el-input>\r\n                    </el-form-item>\r\n                    \r\n                     <el-form-item label=\"仓库名称\" id=\"yy\"  prop=\"mc\" >\r\n                                        <el-select v-model=\"formInline.mc\" clearable filterable id=\"tex\" placeholder=\"请选择\">\r\n                                            <el-option\r\n                                        v-for=\"item in countryList\"\r\n                                        :key=\"item.warehouse_id\"\r\n                                        :label=\"item.ware_code3\"\r\n                                        :value=\"item.ware_code3\"\r\n                                      ></el-option>\r\n                                        </el-select>\r\n                                    </el-form-item>\r\n                                    <el-form-item label=\"联系方式\" id=\"yyy\">\r\n                            <el-input v-model=\"formInline.fs\" id=\"tex\" placeholder=\"请输入\" :disabled=\"true\"></el-input>\r\n                        </el-form-item>\r\n                     <el-form-item label=\"经办人\" id=\"yyy\">\r\n                            <el-select v-model=\"formInline.jbr\" id=\"tex\" clearable filterable placeholder=\"请选择\">\r\n                                <el-option label=\"草稿\" value=\"0\"></el-option>\r\n                                <el-option label=\"待审核\" value=\"1\"></el-option>\r\n                                <el-option label=\"驳回\" value=\"2\"></el-option>\r\n                                <el-option label=\"未入库\" value=\"3\"></el-option>\r\n                                <el-option label=\"部分入库\" value=\"4\"></el-option>\r\n                                <el-option label=\"已完成\" value=\"5\"></el-option>\r\n                                <el-option label=\"已关闭\" value=\"6\"></el-option>\r\n\r\n                            </el-select>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"入库日期\" prop=\"startDate\" id=\"ru\">\r\n                                     <el-date-picker v-model=\"formInline.date\" id=\"tex\" type=\"datetime\" placeholder=\"选择日期时间\" value-format=\"yyyy-MM-dd HH:mm:ss\">\r\n                    </el-date-picker>\r\n                                </el-form-item>\r\n                    <el-form-item label=\"备注\" id=\"yyyy\">\r\n                        <el-input id=\"te\" type=\"textarea\" placeholder=\"请输入\" v-model=\"formInline.bei\" maxlength=\"100\" :rows=\"5\"\r\n                            show-word-limit>\r\n                        </el-input>\r\n                    </el-form-item>\r\n                </el-form>\r\n            </div>\r\n        </div>\r\n        <div id=\"addx\">\r\n            <div style=\"height: 60;line-height: 60px; box-shadow: 0px -1px 7px rgba(0, 0, 0, 0.2);\r\n    background-color: #fff;\">\r\n                <h4 style=\"margin-left: 25px;\">入库明细</h4>\r\n            </div>\r\n            <div style=\"margin-top: 50px;margin-left: 30px;\">\r\n\r\n                <el-table tooltip-effect=\"dark\" @selection-change=\"handleSelectionChange\" :data=\"tableData\"\r\n                    :header-cell-style=\"{ background: '#F8F8F9', color: '#606266', 'text-align': 'center' }\" id=\"ttt\">\r\n                    <!-- <el-table-column type=\"selection\" width=\"55\">\r\n                    </el-table-column> -->\r\n                    <el-table-column\r\n                            label=\"序号\"\r\n                            fixed\r\n              type=\"index\"\r\n              width=\"50\">\r\n            </el-table-column>\r\n\r\n                    <el-table-column fixed prop=\"buy_code\" label=\"货品名称\" width=\"100\">\r\n                    </el-table-column>\r\n                    <el-table-column  fixed label=\"货品编号\" width=\"100\">\r\n                        <template slot-scope=\"scope\">\r\n                            <span v-if=\"scope.row.sale_state == 0\" style=\"font-weight: 600;font-size: 13px;\">草稿</span>\r\n                            <span v-if=\"scope.row.sale_state == 1\"\r\n                                style=\"color: blue;font-weight: 600;font-size: 13px;\">待审核</span>\r\n                            <span v-if=\"scope.row.sale_state == 2\"\r\n                                style=\"color: red;font-weight: 600;font-size: 13px;\">驳回</span>\r\n                            <span v-if=\"scope.row.sale_state == 3\"\r\n                                style=\"color: rgb(109, 128, 0);font-weight: 600;font-size: 13px;\">未入库</span>\r\n                            <span v-if=\"scope.row.sale_state == 4\"\r\n                                style=\"color: rgb(96, 123, 230);font-weight: 600;font-size: 13px;\">部分入库</span>\r\n                            <span v-if=\"scope.row.sale_state == 5\"\r\n                                style=\"color: Green;font-weight: 600;font-size: 13px;\">已完成</span>\r\n                            <span v-if=\"scope.row.sale_state == 6\"\r\n                                style=\"color: black;font-weight: 600;font-size: 13px;\">已关闭</span>\r\n                        </template>\r\n\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"supply_code\" label=\"货品类型\" width=\"80\">\r\n                        </el-table-column>\r\n                    <el-table-column prop=\"supply_code\" label=\"规格型号\" width=\"80\">\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"supply_code\" label=\"单位\" width=\"80\">\r\n                        </el-table-column>\r\n                        <el-table-column prop=\"supply_code\" label=\"当前库存\" width=\"80\">\r\n                        </el-table-column>\r\n                        <el-table-column prop=\"supply_code\" label=\"进货数量\" width=\"80\">\r\n                        </el-table-column>\r\n                        <el-table-column prop=\"supply_code\" label=\"已入库数量\" width=\"140\">\r\n                            </el-table-column>\r\n                            <el-table-column prop=\"supply_code\" label=\"未入库数量\" width=\"140\">\r\n                            </el-table-column>\r\n                    <el-table-column prop=\"link_man\" label=\"入库仓位\" width=\"140\">\r\n                        <el-select v-model=\"formInline.zt\" id=\"bb\" placeholder=\"单据状态\">\r\n                            <el-option label=\"草稿\" value=\"0\"></el-option>\r\n                            <el-option label=\"待审核\" value=\"1\"></el-option>\r\n                            <el-option label=\"驳回\" value=\"2\"></el-option>\r\n                            <el-option label=\"未入库\" value=\"3\"></el-option>\r\n                            <el-option label=\"部分入库\" value=\"4\"></el-option>\r\n                            <el-option label=\"已完成\" value=\"5\"></el-option>\r\n                            <el-option label=\"已关闭\" value=\"6\"></el-option>\r\n\r\n                        </el-select>\r\n                    </el-table-column>\r\n                    \r\n                    <el-table-column prop=\"total_buy_num\" label=\"本次入库数量\" width=\"140\">\r\n                        <el-input v-model=\"formInline.bh\"  id=\"bb\" ></el-input>\r\n                        \r\n                    </el-table-column>\r\n                     <el-table-column  label=\"进货单价\" width=\"100\">\r\n                            </el-table-column>\r\n                            <el-table-column  label=\"进货金额\" width=\"100\">\r\n                            </el-table-column>\r\n                    <el-table-column prop=\"total_price\" label=\"批次号\" width=\"140\">\r\n                        <el-input v-model=\"formInline.bh\" id=\"bb\" placeholder=\"进货单号\"></el-input>\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"names\" label=\"生产日期\" width=\"140\">\r\n     <el-date-picker v-model=\"formInline.date\" type=\"datetime\"  value-format=\"yyyy-MM-dd HH:mm:ss\">  </el-date-picker>\r\n                    </el-table-column>\r\n                    <el-table-column prop=\"create_by\" label=\"备注\" width=\"300\">\r\n                         <el-input v-model=\"formInline.bh\" ></el-input>\r\n                    </el-table-column>\r\n                   \r\n                </el-table>\r\n                <!-- 分页 -->\r\n                <el-pagination id=\"fenye1\" @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\"\r\n                    :current-page=\"currentPage\" :page-sizes=\"[4, 8, 12, 16]\" :page-size=\"pagesize\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\" :total=\"rows\">\r\n                </el-pagination>\r\n            </div>\r\n        </div>\r\n    <!-- 选择进货单 -->\r\n                <el-dialog title=\"选择进货单\" :visible.sync=\"dialogFormVisible\" width=\"800px\" id=\"tjsh\">\r\n\r\n        <el-form :inline=\"true\" :model=\"formtable\" class=\"demo-form-inline\">\r\n          <el-form-item label=\"\" style=\"width:221px\">\r\n            <el-input v-model=\"formtable.dh\"  id=\"bddd\" placeholder=\"请输入进货单号\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"\"  style=\"margin-left: 30px; \">\r\n            <el-select v-model=\"formtable.zt\" clearable filterable placeholder=\"请选择单据状态\" id=\"bddd\">\r\n             <el-option label=\"草稿\" value=\"0\"></el-option>\r\n                                    <el-option label=\"待审核\" value=\"1\"></el-option>\r\n                                    <el-option label=\"驳回\" value=\"2\"></el-option>\r\n                                    <el-option label=\"未入库\" value=