rm-item>\r\n          <el-form-item label=\"排序\" :label-width=\"formLabelWidth\">\r\n            <el-input\r\n              v-model=\"formInline1.px\"\r\n              style=\"width: 300px\"\r\n              autocomplete=\"off\"\r\n              type=\"number\"\r\n              placeholder=\"请输入\"\r\n            ></el-input>\r\n            <p v-if=\"isNaN(formInline1.px)\">必须输入数字</p>\r\n          </el-form-item>\r\n          <el-form-item label=\"备注\" :label-width=\"formLabelWidth\">\r\n            <el-input\r\n              v-model=\"formInline1.bz\"\r\n              autocomplete=\"off\"\r\n              placeholder=\"请输入\"\r\n              style=\"width: 300px\"\r\n            ></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <div class=\"demo-drawer__footer\" id=\"didi\">\r\n          <el-button id=\"tj\" @click=\"xg()\">提交</el-button>\r\n          <el-button @click=\"drawer1 = false\" id=\"qx\">取 消</el-button>\r\n        </div>\r\n      </div>\r\n    </el-drawer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport $http from \"../http/base.js\";\r\nvar tj = \"1 = 1 and is_del = '0'\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableloading: false, //加载\r\n      showDefaultAttr: true,\r\n      multipleSelection: [], // 选中的行数据\r\n      formInline: {\r\n        ck: \"\",\r\n        zt: \"\",\r\n        ren: \"\",\r\n      },\r\n      formInline1: {\r\n        bh: \"\",\r\n        name: \"\",\r\n        rl: \"\",\r\n        zg: \"\",\r\n        dh: \"\",\r\n        px: \"\",\r\n        bz: \"\",\r\n        id: 0,\r\n        bz: \"\",\r\n        mc: \"\",\r\n      },\r\n      formInline2: {\r\n        bh: \"\",\r\n        name: \"\",\r\n        rl: \"\",\r\n        zg: \"\",\r\n        dh: \"\",\r\n        px: \"\",\r\n        bz: \"\",\r\n        bz: \"\",\r\n        mc: \"\",\r\n      },\r\n      tableData: [],\r\n      formLabelWidth: \"100px\",\r\n      drawer: false,\r\n      drawer1: false,\r\n      direction: \"rtl\",\r\n      radio: \"0\",\r\n      radio1: \"0\",\r\n      List: [],\r\n      countryList: [],\r\n      countryList1: [],\r\n      bl: \"1\",\r\n      currentPage: 1, //当前页\r\n      pagesize: 4, //每页显示数\r\n      rows: 0, //总行数\r\n      tj,\r\n      rules: {\r\n        cwmc: [{ required: true, message: \"请输入库位名称!\", trigger: \"blur\" }],\r\n        cwbh: [{ required: true, message: \"请输入库位编号!\", trigger: \"blur\" }],\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    //查询方法\r\n    find() {\r\n      var t = this;\r\n      $http\r\n        .post(\r\n          \"http://localhost:5149/api/Bs_ware_area/FenYeWareArea?currentPage=\" +\r\n            t.currentPage +\r\n            \"&pageSize=\" +\r\n            t.pagesize +\r\n            \"&str=\" +\r\n            tj +\r\n            \"\"\r\n        )\r\n        .then((res) => {\r\n          t.tableData = res.list;\r\n          t.rows = res.rows;\r\n          this.czr();\r\n          this.CK();\r\n          this.CK1();\r\n          console.log(res);\r\n        })\r\n        .catch((err) => {\r\n          alert(err);\r\n        });\r\n    },\r\n    czr() {\r\n      $http\r\n        .post(\"http://localhost:5149/api/SupplyView/Czr\")\r\n        .then((res) => {\r\n          this.List = res;\r\n          console.log(res);\r\n        })\r\n        .catch((err) => {\r\n          alert(err);\r\n        });\r\n    },\r\n    Dang() {\r\n      $http\r\n        .get(\"http://localhost:5149/api/Sys_encode/FindSys_encodeById?id=\" + 23)\r\n        .then((res) => {\r\n          this.formInline2.bh = res;\r\n        })\r\n        .catch((err) => {\r\n          alert(err);\r\n        });\r\n    },\r\n    insert() {\r\n      var t = this;\r\n      var uu = localStorage.getItem(\"name\");\r\n      $http\r\n        .get(\r\n          \"http://localhost:5149/api/Bs_ware_area/AreaName?name=\" +\r\n            t.formInline2.name\r\n        )\r\n        .then((res) => {\r\n          if (res == 0) {\r\n            $http\r\n              .post(\r\n                \"http://localhost:5149/api/Bs_ware_area/InsertArea?code=\" +\r\n                  t.formInline2.bh +\r\n                  \"&name=\" +\r\n                  t.formInline2.name +\r\n                  \"&cap=\" +\r\n                  t.formInline2.rl +\r\n                  \"&man=\" +\r\n                  t.formInline2.zg +\r\n                  \"&px=\" +\r\n                  t.formInline2.px +\r\n                  \"&zt=\" +\r\n                  t.radio +\r\n                  \"&cz=\" +\r\n                  uu +\r\n                  \"&gx=\" +\r\n                  uu +\r\n                  \"&bz=\" +\r\n                  t.formInline2.bz +\r\n                  \"&dh=\" +\r\n                  t.formInline2.dh +\r\n                  \"&cc=\" +\r\n                  t.formInline2.mc +\r\n                  \"\"\r\n              )\r\n              .then((res) => {\r\n                if (res) {\r\n                  this.$message({\r\n                    message: \"添加成功！\",\r\n                    type: \"success\",\r\n                  });\r\n                  t.formInline2.bh = \"\";\r\n                  t.formInline2.name = \"\";\r\n                  t.formInline2.rl = \"\";\r\n                  t.formInline2.zg = \"\";\r\n                  t.formInline2.dh = \"\";\r\n                  t.radio = \"0\";\r\n                  t.formInline2.px = \"\";\r\n                  t.formInline2.bz = \"\";\r\n                  t.formInline2.mc = \"\";\r\n                  t.drawer = false;\r\n                  t.find();\r\n                }\r\n              })\r\n              .catch((err) => {\r\n                console.log(err);\r\n              });\r\n          } else {\r\n            this.$message({\r\n              message: \"库位名称已存在！\",\r\n              type: \"error\",\r\n            });\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    //重置\r\n    reset() {\r\n      this.formInline.ck = \"\";\r\n      this.formInline.zt = \"\";\r\n      this.formInline.ren = \"\";\r\n      tj = \"1=1\";\r\n      this.find();\r\n    },\r\n    xg() {\r\n      var t = this;\r\n      var uu = localStorage.getItem(\"name\");\r\n      $http\r\n        .put(\r\n          \"http://localhost:5149/api/Bs_ware_area/UpdateArea?code=\" +\r\n            t.formInline1.bh +\r\n            \"&name=\" +\r\n            t.formInline1.name +\r\n            \"&cap=\" +\r\n            t.formInline1.rl +\r\n            \"&man=\" +\r\n            t.formInline1.zg +\r\n            \"&px=\" +\r\n            t.formInline1.px +\r\n            \"&zt=\" +\r\n            t.radio1 +\r\n            \"&gx=\" +\r\n            uu +\r\n            \"&bz=\" +\r\n            t.formInline1.bz +\r\n            \"&dh=\" +\r\n            t.formInline1.dh +\r\n            \"&id=\" +\r\n            t.formInline1.id +\r\n            \"&cc=\" +\r\n            t.formInline1.cc +\r\n            \"\"\r\n        )\r\n        .then((res) => {\r\n          if (res) {\r\n            this.$message({\r\n              message: \"修改成功！\",\r\n              type: \"success\",\r\n            });\r\n            t.find();\r\n            t.drawer1 = false;\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    handleEdit() {\r\n      this.drawer = true;\r\n      this.Dang();\r\n    },\r\n    Update(id) {\r\n      var t = this;\r\n      t.drawer1 = true;\r\n      $http\r\n        .get(\"http://localhost:5149/api/Bs_ware_area/SelectID?id=\" + id)\r\n        .then((res) => {\r\n          t.formInline1.bh = res.area_code;\r\n          t.formInline1.name = res.area_name;\r\n          t.formInline1.rl = res.area_cap;\r\n          t.formInline1.zg = res.area_duty_man;\r\n          t.formInline1.dh = res.dh;\r\n          t.radio1 = res.area_state;\r\n          t.formInline1.px = res.sort_code;\r\n          t.formInline1.bz = res.remark;\r\n          t.formInline1.id = res.ware_area_id;\r\n          t.formInline1.mc = res.warehouse_id;\r\n          console.log(res);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    //每页显示数改变事件\r\n    handleSizeChange(size) {\r\n      this.pagesize = size;\r\n      this.currentPage = 1;\r\n      this.find();\r\n      console.log(`每页 ${size} 条`);\r\n    },\r\n    //页码改变事件\r\n    handleCurrentChange(page) {\r\n      this.currentPage = page;\r\n      this.find();\r\n      console.log(`当前页: ${page}`);\r\n    },\r\n    //选择\r\n    handleSelectionChange(val) {\r\n      this.multipleSelection = val;\r\n      console.log(this.multipleSelection);\r\n    },\r\n    //删除\r\n    handleDelete(i) {\r\n      this.$confirm(\"此操作将永久删除该信息, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then(() => {\r\n          console.log(i.item_type_id);\r\n          console.log(this.bl);\r\n          $http\r\n            .post(\r\n              \"http://localhost:5149/api/WareArea/AreaDelete?id=\" +\r\n                i.ware_area_id +\r\n                \"&str=\" +\r\n                this.bl\r\n            )\r\n            .then((res) => {\r\n              if (res) {\r\n                this.$message({\r\n                  message: \"删除成功！\",\r\n                  type: \"success\",\r\n                });\r\n                this.find();\r\n              }\r\n            })\r\n            .catch((err) => {\r\n              console.log(err);\r\n            });\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: \"已取消删除\",\r\n          });\r\n        });\r\n    },\r\n    //刷新table\r\n    flushed() {\r\n      this.tableloading = true;\r\n      // 模拟获取接口数据\r\n      setTimeout(() => {\r\n        //调用find();this.find();\r\n        this.tableloading = false;\r\n        this.find();\r\n      }, 1000);\r\n      //直接this.查询方法