{"ast":null,"code":"import $http from '../http/base.js';\nexport default {\n  data() {\n    return {\n      str: \"1=1\",\n      dialogVisible: true,\n      dialogFormVisible: false,\n      dialogFormVisible1: false,\n      gridData: [],\n      gridData1: [],\n      multipleSelection: [],\n      // 选中的行数据\n      multipleSelection1: [],\n      // 选中的行数据\n      currentPage: 1,\n      //当前页\n      pagesize: 4,\n      //每页显示数\n      currentPage1: 1,\n      //当前页\n      pagesize1: 4,\n      //每页显示数\n      rows: 0,\n      //总行数\n      rows1: 0,\n      tableData: [],\n      formtable1: {\n        bh: \"\",\n        dw: \"\"\n      },\n      formtable: {\n        dh: \"\",\n        zt: \"\",\n        gys: \"\",\n        jhr: \"\",\n        data: \"\"\n      },\n      xx: {\n        wei: 0,\n        cw: \"\",\n        sl: \"\",\n        bei: \"\",\n        jin: 0\n      },\n      ff: false,\n      countryList: [],\n      countryList1: [],\n      countryList2: [],\n      formInline: {\n        dh: '',\n        lx: '',\n        mc: '',\n        date: '',\n        bei: \"\",\n        lxr: \"\",\n        fs: \"\",\n        gldh: \"\",\n        gys: \"\",\n        jin: \"\"\n      },\n      rules: {\n        startDate: [{\n          required: true,\n          message: \"请选择入库时间!\",\n          trigger: \"blur\"\n        }],\n        dh: [{\n          required: true,\n          message: \"请填写单号!\",\n          trigger: \"blur\"\n        }],\n        lx: [{\n          required: true,\n          message: \"请选择入库类型!\",\n          trigger: \"change\"\n        }],\n        mc: [{\n          required: true,\n          message: \"请选择仓库名称!\",\n          trigger: \"change\"\n        }]\n      },\n      rules1: {\n        link_man: [{\n          required: true,\n          message: \"请选择入库时间!\",\n          trigger: \"blur\"\n        }]\n      }\n    };\n  },\n  methods: {\n    cang() {\n      var t = this;\n      $http.post(\"http://localhost:5149/api/Wh_inbill_h/Bs_ware_areaName?bh=\" + this.formInline.mc).then(res => {\n        t.countryList1 = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    zt(i) {\n      if (i == 0) return \"草稿\";\n      if (i == 1) return \"待审核\";\n      if (i == 2) return \"驳回\";\n      if (i == 3) return \"已完成\";\n    },\n    //查询方法\n    find() {\n      var t = this;\n      $http.post(\"http://localhost:5149/api/Wh_inbill_h/FenOrder?page=\" + t.pagesize + \"&pageSize=\" + t.currentPage + \"&str=\" + t.str).then(res => {\n        t.gridData = res.list;\n        t.rows1 = res.rows;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //进货查询\n    onSubmit() {\n      this.str = \"1=1 and is_del=0 \";\n      if (this.formtable.dh != \"\") {\n        this.str += \"and buy_code like '%\" + this.formtable.dh + \"%'\";\n      }\n      if (this.formtable.zt != \"\") {\n        this.str += \"and sale_state like '%\" + this.formtable.zt + \"%'\";\n      }\n      if (this.formtable.gys != \"\") {\n        this.str += \"and supply_code like '%\" + this.formtable.gys + \"%'\";\n      }\n      if (this.formtable.data != \"\") {\n        this.str += \"and buy_date = '\" + this.formtable.data + \"'\";\n      }\n      if (this.formtable.jhr != \"\") {\n        this.str += \"and buy_man = '\" + this.formtable.jhr + \"'\";\n      }\n      alert(this.str);\n      this.find();\n    },\n    //下拉框\n    CK() {\n      var t = this;\n      $http.get(\"http://localhost:5149/api/Bs_warehouse/Bs_warehouseSelect\").then(res => {\n        t.countryList = res;\n      }).catch(err => {\n        alert(err);\n      });\n      $http.get(\"http://localhost:5149/api/Wh_inbill_h/FindUser\").then(res => {\n        t.countryList1 = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //选择单号\n    xz() {\n      this.dialogFormVisible = true;\n      $http.get(\"http://localhost:5149/api/Wh_inbill_h/FindUser\").then(res => {\n        this.countryList2 = res;\n      }).catch(err => {\n        alert(err);\n      });\n      this.find();\n    },\n    filterTime(timestamp) {\n      return formatDate(timestamp);\n    },\n    //保存\n    bc() {},\n    //添加\n    bcc() {\n      this.dialogFormVisible1 = true;\n      var t = this;\n      $http.post(\"http://localhost:5149/api/Wh_inbill_h/Wh_orderbuy_dS\").then(res => {\n        t.gridData1 = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //取消添加\n    qqxx() {\n      this.dialogFormVisible1 = false;\n    },\n    //关闭\n    gb() {\n      //this.$router.push('/Wh_inbill_h');\n      this.$router.go(-1);\n    },\n    //删除\n    sc() {\n      alert(\"123\");\n    },\n    handleRowClick(i) {\n      if (i.sale_state != 2) {\n        this.$notify({\n          title: '警告',\n          message: '请选择已完成的订单！',\n          type: 'warning'\n        });\n        return;\n      }\n      this.formInline.gldh = i.buy_code;\n      this.formInline.gys = i.supply_code;\n      this.formInline.lxr = i.link_man;\n      this.formInline.fs = i.link_way;\n      this.dialogFormVisible = false;\n      this.ff = true;\n      var t = this;\n      $http.post(\" http://localhost:5149/api/Wh_inbill_h/Wh_orderbuy_dBH?bh=\" + i.buy_code).then(res => {\n        t.tableData = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //每页显示数改变事件\n    handleSizeChange(size) {\n      this.pagesize = size;\n      this.currentPage = 1;\n      // this.find();\n    },\n\n    //页码改变事件\n    handleCurrentChange(page) {\n      this.currentPage = page;\n      // this.find();\n    },\n\n    //每页显示数改变事件\n    handleSizeChange1(size) {\n      this.pagesize1 = size;\n      this.currentPage1 = 1;\n\n      // this.find();\n    },\n\n    //页码改变事件\n    handleCurrentChange1(page) {\n      this.currentPage1 = page;\n      // this.find();\n    },\n\n    //选择\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n    //选择-添加\n    handleSelectionChange1(val) {\n      this.multipleSelection1 = val;\n      this.tableData = val;\n      console.log(this.multipleSelection1);\n    },\n    //重置\n    cz() {\n      this.formtable = {\n        dh: \"\",\n        zt: \"\",\n        gys: \"\",\n        jhr: \"\",\n        data: \"\"\n      };\n    }\n  },\n  mounted() {\n    this.CK();\n  }\n};\nexport function formatDate(date) {\n  let time = new Date(date);\n  let year = time.getFullYear();\n  let month = time.getMonth() + 1;\n  month = month.toString().padStart(2, \"0\");\n  let day = time.getDate().toString().padStart(2, \"0\");\n  return year + '年' + month + '月' + day + '日';\n}","map":{"version":3,"names":["$http","data","str","dialogVisible","dialogFormVisible","dialogFormVisible1","gridData","gridData1","multipleSelection","multipleSelection1","currentPage","pagesize","currentPage1","pagesize1","rows","rows1","tableData","formtable1","bh","dw","formtable","dh","zt","gys","jhr","xx","wei","cw","sl","bei","jin","ff","countryList","countryList1","countryList2","formInline","lx","mc","date","lxr","fs","gldh","rules","startDate","required","message","trigger","rules1","link_man","methods","cang","t","post","then","res","catch","err","alert","i","find","list","onSubmit","CK","get","xz","filterTime","timestamp","formatDate","bc","bcc","qqxx","gb","$router","go","sc","handleRowClick","sale_state","$notify","title","type","buy_code","supply_code","link_way","handleSizeChange","size","handleCurrentChange","page","handleSizeChange1","handleCurrentChange1","handleSelectionChange","val","console","log","handleSelectionChange1","cz","mounted","time","Date","year","getFullYear","month","getMonth","toString","padStart","day","getDate"],"sources":["src/components/AddWh_inbill_h.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <div id=\"adds\">\r\n            <div style=\"height: 60;line-height: 60px; box-shadow: 0px -1px 7px rgba(0, 0, 0, 0.2);\r\n    background-color: #fff;\">\r\n                <h4 style=\"margin-left: 25px;\">基础信息</h4>\r\n            </div>\r\n            <div style=\"margin-top: 30px;\">\r\n                <el-form :inline=\"true\" :model=\"formInline\" :rules=\"rules\" class=\"demo-form-inline\">\r\n                    <el-form-item label=\"入库单号\" id=\"yy\" prop=\"dh\">\r\n                        <el-input v-model=\"formInline.dh\" id=\"tex\" placeholder=\"自动获取系统编号\"  :readonly=\"true\"></el-input>\r\n                    </el-form-item>\r\n                    \r\n                         <el-form-item label=\"关联单号\" id=\"yyy\">\r\n                               <el-input v-model=\"formInline.gldh\" id=\"tex\" placeholder=\"请输入\" @focus=\"xz()\"></el-input>\r\n                              \r\n                            </el-form-item>\r\n                           \r\n                    <el-form-item label=\"供应商\" id=\"yyy\">\r\n                         <el-select v-model=\"formInline.gys\" id=\"tex\" :disabled=\"ff\" clearable filterable placeholder=\"请选择\">\r\n                                            <el-option label=\"进货入库\" value=\"进货入库\"></el-option>\r\n                                            <el-option label=\"生产入库\" value=\"生产入库\"></el-option>\r\n                                             <el-option label=\"退货入库\" value=\"退货入库\"></el-option>\r\n                                              <el-option label=\"换货入库\" value=\"换货入库\"></el-option>\r\n                                        </el-select>\r\n                       \r\n                    </el-form-item>\r\n                    <el-form-item label=\"入库类型\"   id=\"yy\" prop=\"lx\">\r\n                                    <el-select v-model=\"formInline.lx\" id=\"tex\" clearable filterable placeholder=\"请选择\">\r\n                                        <el-option label=\"进货入库\" value=\"进货入库\"></el-option>\r\n                                        <el-option label=\"生产入库\" value=\"生产入库\"></el-option>\r\n                                         <el-option label=\"退货入库\" value=\"退货入库\"></el-option>\r\n                                          <el-option label=\"换货入库\" value=\"换货入库\"></el-option>\r\n                                    </el-select>\r\n                                </el-form-item>\r\n                    <el-form-item label=\"进货日期\" id=\"yyy\" >\r\n                         <el-date-picker v-model=\"formInline.jin\" id=\"tex\" type=\"datetime\" placeholder=\"选择日期时间\" value-format=\"yyyy-MM-dd HH:mm:ss\"></el-date-picker>\r\n                      \r\n                    </el-form-item>  \r\n                    <el-form-item label=\"联系人\" id=\"yy1\">\r\n                        <el-input v-model=\"formInline.lxr\" id=\"tex\" placeholder=\"请输入\" :disabled=\"true\"></el-input>\r\n                    </el-form-item>\r\n                    \r\n                     <el-form-item label=\"仓库名称\" id=\"yy\"  prop=\"mc\" >\r\n                                        <el-select v-model=\"formInline.mc\" clearable filterable id=\"tex\" placeholder=\"请选择\" @change=\"cang()\" >\r\n                                            <el-option\r\n                                        v-for=\"item in countryList\"\r\n                                        :key=\"item.warehouse_id\"\r\n                                        :label=\"item.ware_name\"\r\n                                        :value=\"item.warehouse_id\"\r\n                                       \r\n                                      ></el-option>\r\n                                        </el-select>\r\n                                    </el-form-item>\r\n                                    <el-form-item label=\"联系方式\" id=\"yyy\">\r\n                            <el-input v-model=\"formInline.fs\" id=\"tex\" placeholder=\"请输入\" :disabled=\"true\"></el-input>\r\n                        </el-form-item>\r\n                     <el-form-item label=\"经办人\" id=\"yyy\">\r\n                            <el-select v-model=\"formInline.jbr\" id=\"tex\" clearable filterable placeholder=\"请选择\">\r\n                               <el-option\r\n  