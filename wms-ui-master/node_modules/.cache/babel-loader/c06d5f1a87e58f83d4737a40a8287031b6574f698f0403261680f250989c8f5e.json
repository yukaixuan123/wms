{"ast":null,"code":"import $http from \"@/http/base\";\nvar str = \"1=1 and is_del=0\";\nexport function formatDate(date) {\n  let time = new Date(date);\n  let year = time.getFullYear();\n  let month = time.getMonth() + 1;\n  month = month.toString().padStart(2, \"0\");\n  let day = time.getDate().toString().padStart(2, \"0\");\n  return year + \"年\" + month + \"月\" + day + \"日\";\n}\nexport default {\n  data() {\n    return {\n      activeIndex: \"1\",\n      formInline: {\n        cust_code: \"\",\n        cust_name: \"\",\n        e_mail: \"\",\n        link_tel: \"\",\n        cust_adr: \"\",\n        cust_from: \"\",\n        link_man: \"\",\n        line_type: \"\",\n        head_of_Sales: \"\",\n        remark: \"\",\n        finance_name: \"\",\n        tax_no: \"\",\n        bank_no: \"\",\n        open_bank: \"\",\n        open_adr: \"\",\n        zt: \"\"\n      },\n      tableData: [],\n      tableData1: [],\n      tableData2: [],\n      table1: [],\n      ii: \"\",\n      currentPage: 1,\n      //当前页\n      pagesize: 4,\n      //每页显示数\n      rows: 0,\n      //总行数\n      multipleSelection: [],\n      yrksl: 0,\n      zje: 0,\n      activeName: \"first\"\n    };\n  },\n  methods: {\n    zt(i) {\n      if (i == 0) return \"启用\";\n      if (i == 1) return \"停用\";\n    },\n    //每页显示数改变事件\n    handleSizeChange(size) {\n      this.pagesize = size;\n      this.currentPage = 1;\n      this.FenYe();\n    },\n    //页码改变事件\n    handleCurrentChange(page) {\n      this.currentPage = page;\n      this.FenYe();\n    },\n    FenYe() {\n      var t = this;\n      str = \"cust_code='\" + this.$route.query.dh + \"'\";\n      $http.get(\"http://localhost:5149/api/Wh_sale_h/Wh_sale_hFenYe\", {\n        params: {\n          pageSize: t.pagesize,\n          currentPage: t.currentPage,\n          str\n        }\n      }).then(res => {\n        console.log(res);\n        t.table1 = res.list;\n        t.rows = res.rows;\n        console.log(str);\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    //关闭\n    gb() {\n      this.$router.go(-1);\n    },\n    fzcg() {\n      this.$message({\n        message: \"复制成功\",\n        type: \"success\"\n      });\n    },\n    //下标\n    handleSelect(key, keyPath) {\n      console.log(key, keyPath);\n    },\n    //选择\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n    //根据id查进货订单\n    Jia() {\n      var t = this;\n      var bh = this.$route.query.dh;\n      $http.get(\"http://localhost:5149/api/CustomerView/SelectCode?id=\" + bh).then(res => {\n        t.formInline.cust_code = res.cust_code;\n        t.formInline.cust_name = res.cust_name;\n        t.formInline.link_man = res.link_man;\n        t.formInline.link_tel = res.link_tel;\n        t.formInline.e_mail = res.e_mail;\n        t.formInline.cust_adr = res.cust_adr;\n        t.formInline.zt = res.inbill_state;\n        t.formInline.cust_from = res.cust_from;\n        t.formInline.remark = res.remark;\n        t.formInline.line_type = res.line_type;\n        t.formInline.head_of_Sales = res.head_of_Sales;\n        t.formInline.finance_name = res.finance_name;\n        t.formInline.tax_no = res.tax_no;\n        t.formInline.bank_no = res.bank_no;\n        t.formInline.open_bank = res.open_bank;\n        t.formInline.open_adr = res.open_adr;\n        console.log(res);\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    //进货明细\n    find() {\n      var t = this;\n      str = \"cust_code='\" + this.$route.query.dh + \"'\";\n      $http.get(\"http://localhost:5149/api/CustomerView/FenYekhCK\", {\n        params: {\n          pageSize: t.pagesize,\n          currentPage: t.currentPage,\n          str\n        }\n      }).then(res => {\n        console.log(res);\n        t.tableData2 = res.list;\n        t.rows = res.rows;\n        console.log(t.formInline.supply_name);\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    //时间格式化\n    filterTime(timestamp) {\n      return formatDate(timestamp);\n    },\n    handleClick(tab, event) {\n      console.log(tab, event);\n    }\n  },\n  mounted() {\n    this.Jia();\n    this.find();\n    this.FenYe();\n  }\n};","map":{"version":3,"names":["$http","str","formatDate","date","time","Date","year","getFullYear","month","getMonth","toString","padStart","day","getDate","data","activeIndex","formInline","cust_code","cust_name","e_mail","link_tel","cust_adr","cust_from","link_man","line_type","head_of_Sales","remark","finance_name","tax_no","bank_no","open_bank","open_adr","zt","tableData","tableData1","tableData2","table1","ii","currentPage","pagesize","rows","multipleSelection","yrksl","zje","activeName","methods","i","handleSizeChange","size","FenYe","handleCurrentChange","page","t","$route","query","dh","get","params","pageSize","then","res","console","log","list","catch","err","gb","$router","go","fzcg","$message","message","type","handleSelect","key","keyPath","handleSelectionChange","val","Jia","bh","inbill_state","find","supply_name","filterTime","timestamp","handleClick","tab","event","mounted"],"sources":["src/components/CustomerXiang.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div id=\"addss\">\r\n      <h3 style=\"margin-left: 35px; padding-top: 30px; display: inline-block\">\r\n        客户单号：{{ formInline.cust_code }}\r\n        <img\r\n          src=\"../Images/fz.png\"\r\n          width=\"20px\"\r\n          height=\"20px\"\r\n          style=\"vertical-align: middle; margin-left: 120px\"\r\n          @click=\"fzcg\"\r\n        />\r\n      </h3>\r\n\r\n      <div style=\"float: right; padding-top: 30px; margin-right: 35px\">\r\n        <!-- <el-button id=\"tex3\" @click=\"inserok()\" icon=\"el-icon-plus\">编辑</el-button> -->\r\n        <!-- <el-button  id=\"tex4\"  icon=\"el-icon-edit\">修改</el-button> -->\r\n        <el-button id=\"tex5\" @click=\"delAll()\" icon=\"el-icon-delete\"\r\n          >撤销</el-button\r\n        >\r\n        <el-button id=\"tex6\" icon=\"el-icon-download\">打印</el-button>\r\n      </div>\r\n\r\n      <table\r\n        style=\"\r\n          width: 1100px;\r\n          line-height: 55px;\r\n          margin-left: 35px;\r\n          padding-top: 20px;\r\n        \"\r\n      >\r\n        <tr>\r\n          <td>\r\n            所属行业<i style=\"margin-left: 50px; color: #999999\">{{\r\n              formInline.line_type\r\n            }}</i>\r\n          </td>\r\n\r\n          <td>\r\n            客户来源<i style=\"margin-left: 50px; color: #999999\">{{\r\n              formInline.cust_from\r\n            }}</i>\r\n          </td>\r\n\r\n          <td>\r\n            联系人<i style=\"margin-left: 50px; color: #999999\">{{\r\n              formInline.link_man\r\n            }}</i>\r\n          </td>\r\n\r\n          <td>\r\n            状态<i style=\"margin-left: 50px; font-weight: bold\">{{\r\n              zt(formInline.zt)\r\n            }}</i>\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>\r\n            联系电话<i style=\"margin-left: 50px; color: #999999\">{{\r\n              formInline.link_tel\r\n            }}</i>\r\n          </td>\r\n          <td>\r\n            电子邮箱<i style=\"margin-left: 50px; color: #999999\">{{\r\n              formInline.e_mail\r\n            }}</i>\r\n          </td>\r\n          <td>\r\n            销售负责人<i style=\"margin-left: 50px; color: #999999\">{{\r\n              formInline.head_of_Sales\r\n            }}</i>\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>\r\n            客户地址<i style=\"margin-left: 50px; color: #999999\">{{\r\n              formInline.cust_adr\r\n            }}</i>\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>\r\n            备注<i style=\"margin-left: 65px; color: #999999\">{{\r\n              formInline.remark\r\n            }}</i>\r\n          </td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n\r\n    <div id=\"addxx\">\r\n      <el-tabs id=\"menu\" v-model=\"activeName\" @tab-click=\"handleClick\">\r\n        <el-tab-pane label=\"基本信息\" style=\"margin-top: -10px\" name=\"first\">\r\n          <h3\r\n            style=\"margin-left: 35px; padding-top: 30px; display: inline-block\"\r\n          >\r\n            财务信息\r\n          </h3>\r\n          <table\r\n            style=\"\r\n              width: 1100px;\r\n              line-height: 55px;\r\n              margin-left: 35px;\r\n              padding-top: 20px;\r\n            \"\r\n          >\r\n            <tr>\r\n              <td>\r\n                账户名称<i style=\"margin-left: 50px; color: #999999\">{{\r\n                  formInline.finance_name\r\n                }}</i>\r\n              </td>\r\n\r\n              <td>\r\n                纳税人识别号<i style=\"margin-left: 50px; color: #999999\">{{\r\n                  formInline.tax_no\r\n                }}</i>\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <td>\r\n                银行账户<i style=\"margin-left: 50px; color: #999999\">{{\r\n                  formInline.bank_no\r\n                }}</i>\r\n              </td>\r\n\r\n              <td>\r\n                开户银行<i style=\"margin-left: 50px; color: #999999\">{{\r\n                  formInline.open_bank\r\n                }}</i>\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <td>\r\n                开户行地址<i style=\"margin-left: 50px; color: #999999\">{{\r\n                  formInline.open_adr\r\n                }}</i>\r\n              </td>\r\n            </tr>\r\n          </table>\r\n        </el-tab-pane>\r\n        <el-tab-pane label=\"销售记录\" name=\"third\">\r\n          <el-button id=\"tex9\" icon=\"el-icon-download\">导出</el-button>\r\n          <el-table\r\n            ttooltip-effect=\"dark\"\r\n            :data=\"table1\"\r\n            :header-cell-style=\"{\r\n              background: '#F8F8F9',\r\n              color: '#606266',\r\n              'text-align': 'center',\r\n            }\"\r\n            :cell-style=\"{ 'text-align': 'center' }\"\r\n            id=\"jhmx\"\r\n            height=\"360px\"\r\n          >\r\n            <el-table-column type=\"selection\" width=\"55\"> </el-table-column>\r\n            <el-table-column\r\n              fixed\r\n              prop=\"sale_codeId\"\r\n              label=\"序号\"\r\n              type=\"index\"\r\n              width=\"50\"\r\n            >\r\n            </el-table-column>\r\n            <el-table-column\r\n              fixed\r\n              prop=\"sale_code\"\r\n              label=\"销售单号\"\r\n              width=\"100\"\r\n            >\r\n            </el-table-column>\r\n            <el-table-column\r\n              fixed\r\n              prop=\"sale_state\"\r\n              label=\"单据状态\"\r\n              width=\"100\"\r\n            >\r\n              <template slot-scope=\"scope\">\r\n                <span\r\n                  v-if=\"scope.row.sale_state == 0\"\r\n                  style=\"color: red; font-weight: 600; font-size: 13px\"\r\n                  >待审核</span\r\n                >\r\n                <span\r\n                  v-if=\"scope.row.sale_state > 1\"\r\n                  style=\"color: blue; font-weight: 600; font-size: 13px\"\r\n                  >已完成</span\r\n                >\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column\r\n              fixed\r\n              prop=\"sale_date\"\r\n              label=\"销售日期\"\r\n              width=\"140\"\r\n            >\r\n              <template slot-scope=\"scope\">\r\n                <span v-if=\"scope.row.sale_date != '0001-01-01T00:00:00'\">{{\r\n                  filterTime(scope.row.sale_date)\r\n                }}</span>\r\n                <span v-else></span>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"link_man\" label=\"联系人\" width=\"100px\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"link_way\" label=\"联系方式\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"sale_duty_man\"\r\n              label=\"销售负责人\"\r\n              width=\"100\"\r\n            >\r\n            </el-table-column>\r\n            <el-table-column prop=\"total_buy_num\" label=\"销售数量\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"total_price\" label=\"销售金额\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"names\" label=\"销售货品\" width=\"200\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"create_by\" label=\"制单人\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"create_time\" label=\"制单时间\" width=\"140\">\r\n              <template slot-scope=\"scope\">\r\n                <span v-if=\"scope.row.create_time != '0001-01-01T00:00:00'\">{{\r\n                  filterTime(scope.row.create_time)\r\n                }}</span>\r\n                <span v-else></span>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"sure_man\" label=\"审核人\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"sure_date\" label=\"审核时间\" width=\"140\">\r\n              <template slot-scope=\"scope\">\r\n                <span v-if=\"scope.row.sure_date != '0001-01-01T00:00:00'\">{{\r\n                  filterTime(scope.row.sure_date)\r\n                }}</span>\r\n                <span v-else></span>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n          <!-- 分页 -->\r\n          <el-pagination\r\n            id=\"fenye\"\r\n            @size-change=\"handleSizeChange\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"currentPage\"\r\n            :page-sizes=\"[4, 8, 12, 16]\"\r\n            :page-size=\"pagesize\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n            :total=\"rows\"\r\n          >\r\n          </el-pagination>\r\n        </el-tab-pane>\r\n\r\n        <el-tab-pane label=\"出库记录\" name=\"third1\">\r\n          <el-button id=\"tex9\" icon=\"el-icon-download\">导出</el-button>\r\n          <el-table\r\n            tooltip-effect=\"dark\"\r\n            :data=\"tableData2\"\r\n            :header-cell-style=\"{\r\n              background: '#F8F8F9',\r\n              color: '#606266',\r\n              'text-align': 'center',\r\n            }\"\r\n            :cell-style=\"{ 'text-align': 'center' }\"\r\n            id=\"jhmx\"\r\n            height=\"300px\"\r\n          >\r\n            <el-table-column type=\"selection\" width=\"55\"> </el-table-column>\r\n            <el-table-column label=\"序号\" type=\"index\" width=\"70\">\r\n            </el-table-column>\r\n\r\n            <el-table-column prop=\"outbill_code\" label=\"出库单号\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"outbill_type\" label=\"出库类型\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"outbill_date\" label=\"出库日期\" width=\"140\">\r\n              <template slot-scope=\"scope\">\r\n                <span v-if=\"scope.row.outbill_date != '0001-01-01T00:00:00'\">{{\r\n                  filterTime(scope.row.outbill_date)\r\n                }}</span>\r\n                <span v-else></span>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"ware_code\" label=\"仓库名称\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"outbill_num\" label=\"出库总数\" width=\"80\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"zong\" label=\"销售总金额\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"duty_man\" label=\"经办人\" width=\"80\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"create_by\" label=\"制单人\" width=\"80\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"create_time\" label=\"制单时间\" width=\"140\">\r\n              <template slot-scope=\"scope\">\r\n                <span v-if=\"scope.row.create_time != '0001-01-01T00:00:00'\">{{\r\n                  filterTime(scope.row.create_time)\r\n                }}</span>\r\n                <span v-else></span>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n\r\n          <!-- 分页 -->\r\n          <el-pagination\r\n            id=\"fenye\"\r\n            @size-change=\"handleSizeChange\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"currentPage\"\r\n            :page-sizes=\"[4, 8, 12, 16]\"\r\n            :page-size=\"pagesize\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n            :total=\"rows\"\r\n          >\r\n          </el-pagination>\r\n        </el-tab-pane>\r\n      </el-tabs>\r\n    </div>\r\n    <div id=\"notification-container\">\r\n      <div id=\"dibu\">\r\n        <el-button id=\"bcccc\" @click=\"gb()\">关闭</el-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport $http from \"@/http/base\";\r\n\r\nvar str = \"1=1 and is_del=0\";\r\nexport function formatDate(date) {\r\n  let time = new Date(date);\r\n\r\n  let year = time.getFullYear();\r\n\r\n  let month = time.getMonth() + 1;\r\n  month = month.toString().padStart(2, \"0\");\r\n  let day = time.getDate().toString().padStart(2, \"0\");\r\n  return year + \"年\" + month + \"月\" + day + \"日\";\r\n}\r\nexport default {\r\n  data() {\r\n    return {\r\n      activeIndex: \"1\",\r\n\r\n      formInline: {\r\n        cust_code: \"\",\r\n        cust_name: \"\",\r\n        e_mail: \"\",\r\n        link_tel: \"\",\r\n        cust_adr: \"\",\r\n        cust_from: \"\",\r\n        link_man: \"\",\r\n        line_type: \"\",\