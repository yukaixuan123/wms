{"ast":null,"code":"import $http from \"../http/base.js\";\nvar tj = \"1 = 1 and is_del = '0'\";\nexport default {\n  data() {\n    return {\n      tableloading: false,\n      //加载\n      showDefaultAttr: true,\n      multipleSelection: [],\n      // 选中的行数据\n      formInline: {\n        kh: '',\n        zt: '',\n        hy: '',\n        ren: ''\n      },\n      tableData: [],\n      List1: [],\n      currentPage: 1,\n      //当前页\n      pagesize: 3,\n      //每页显示数\n      rows: 0,\n      //总行数\n      tj,\n      List: [],\n      str: '1',\n      ss: []\n    };\n  },\n  methods: {\n    //查询方法\n    find() {\n      var t = this;\n      $http.post(\"http://localhost:5149/api/CustomerView/FenYeCustomer?currentPage=\" + t.currentPage + \"&pageSize=\" + t.pagesize + \"&str=\" + tj + \"\").then(res => {\n        t.tableData = res.list;\n        t.rows = res.rows;\n        t.lxr();\n        t.hy();\n        console.log(\"----\");\n        console.log(res);\n        console.log(\"----\");\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    lxr() {\n      $http.post(\"http://localhost:5149/api/SupplyView/Czr\").then(res => {\n        this.List = res;\n        console.log(res);\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //重置\n    resetForm() {\n      this.formInline.kh = \"\";\n      this.formInline.hy = \"\";\n      this.formInline.zt = \"\";\n      this.formInline.ren = \"\";\n      tj = \"1=1\";\n      this.find();\n    },\n    handleEdit() {\n      this.$router.push('customerInsert');\n    },\n    update(i) {\n      this.$router.push('customerUpdate?id=' + i);\n    },\n    //搜索\n    search() {\n      tj = \"1=1\";\n      if (this.formInline.kh != \"\") {\n        tj += \" and cust_Name like '%\" + this.formInline.kh + \"%' or cust_code like '%\" + this.formInline.kh + \"%'\";\n      }\n      if (this.formInline.zt != \"\") {\n        tj += \" and inbill_state like '%\" + this.formInline.zt + \"%'\";\n      }\n      if (this.formInline.hy != \"\") {\n        tj += \" and line_type like '%\" + this.formInline.hy + \"%'\";\n      }\n      if (this.formInline.ren != \"\") {\n        tj += \" and link_man like '%\" + this.formInline.ren + \"%'\";\n      }\n      this.find();\n    },\n    //选择\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n    //删除\n    handleDelete(i) {\n      this.$confirm('此操作将永久删除该信息, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        $http.post(\"http://localhost:5149/api/CustomerView/CustomerDelete?id=\" + i.customer_id + \"&str=\" + this.str).then(res => {\n          if (res) {\n            this.$message({\n              message: '删除成功！',\n              type: 'success'\n            });\n            this.find();\n          }\n        }).catch(err => {\n          console.log(err);\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'error',\n          message: '已取消删除'\n        });\n      });\n    },\n    //每页显示数改变事件\n    handleSizeChange(size) {\n      this.pagesize = size;\n      this.currentPage = 1;\n      this.find();\n      console.log(`每页 ${size} 条`);\n    },\n    //页码改变事件\n    handleCurrentChange(page) {\n      this.currentPage = page;\n      this.find();\n      console.log(`当前页: ${page}`);\n    },\n    //刷新table\n    flushed() {\n      this.tableloading = true;\n      // 模拟获取接口数据\n      setTimeout(() => {\n        //调用find();this.find();\n        this.tableloading = false;\n        this.find();\n      }, 1000);\n      //直接this.查询方法();\n    },\n\n    hy() {\n      $http.post('http://localhost:5149/api/CustomerView/HY').then(res => {\n        this.List1 = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //批量删除\n    delAll() {\n      this.$confirm('此操作将永久删除该信息, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        const length = this.multipleSelection.length;\n        var ss = [];\n        for (let i = 0; i < this.multipleSelection.length; i++) {\n          ss.push(this.multipleSelection[i].customer_id);\n        }\n        console.log('lo' + length);\n        $http.put('http://localhost:5149/api/CustomerView/PLDeleteCustomer', ss).then(res => {\n          if (res) {\n            this.$message({\n              message: '删除成功！',\n              type: 'success'\n            });\n            this.find();\n          }\n        }).catch(err => {\n          console.log(err);\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'error',\n          message: '已取消删除'\n        });\n      });\n    },\n    filterTime(timestamp) {\n      return formatDate(timestamp);\n    }\n  },\n  mounted() {\n    this.tableloading = true;\n    // 模拟获取接口数据\n    setTimeout(() => {\n      //调用find();\n      this.find();\n      this.tableloading = false;\n    }, 500);\n  }\n};\nexport function formatDate(date) {\n  let time = new Date(date);\n  let year = time.getFullYear();\n  let month = time.getMonth() + 1;\n  month = month.toString().padStart(2, \"0\");\n  let day = time.getDate().toString().padStart(2, \"0\");\n  return year + '年' + month + '月' + day + '日';\n}","map":{"version":3,"names":["$http","tj","data","tableloading","showDefaultAttr","multipleSelection","formInline","kh","zt","hy","ren","tableData","List1","currentPage","pagesize","rows","List","str","ss","methods","find","t","post","then","res","list","lxr","console","log","catch","err","alert","resetForm","handleEdit","$router","push","update","i","search","handleSelectionChange","val","handleDelete","$confirm","confirmButtonText","cancelButtonText","type","customer_id","$message","message","handleSizeChange","size","handleCurrentChange","page","flushed","setTimeout","delAll","length","put","filterTime","timestamp","formatDate","mounted","date","time","Date","year","getFullYear","month","getMonth","toString","padStart","day","getDate"],"sources":["src/components/Customer.vue"],"sourcesContent":["<template>\r\n    <div id=\"kj\">\r\n        <!--头部-->\r\n        <el-breadcrumb separator=\"/\" id=\"card\">\r\n            <el-breadcrumb-item :to=\"{ path: '/Home' }\">首页</el-breadcrumb-item>\r\n            <el-breadcrumb-item><a href=\"javascript:void(0)\">来往单位</a></el-breadcrumb-item>\r\n            <el-breadcrumb-item>客户管理</el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n        <!--搜索-->\r\n\r\n        <transition-group name=\"lyric\">\r\n\r\n            <div id=\"form\" v-if=\"showDefaultAttr\" key=\"1\">\r\n                <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n                    <el-form-item label=\"客户\" id=\"form_top\">\r\n                        <el-input v-model=\"formInline.kh\" id=\"tex\" placeholder=\"请输入客户名称/编号\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"状态\" id=\"form_top\">\r\n                        <el-select v-model=\"formInline.zt\" id=\"tex\" placeholder=\"请选择\">\r\n                            <el-option label=\"启用\" value=\"0\"></el-option>\r\n                            <el-option label=\"停用\" value=\"1\"></el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"所属行业\" id=\"form_top\">\r\n                        <el-select v-model=\"formInline.hy\" id=\"tex\" placeholder=\"请选择\">\r\n                            <el-option v-for=\"item in List1\" :key=\"item.line_type\" :label=\"item.line_type\"\r\n                                :value=\"item.line_type\"></el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"联系人\" prop=\"startDate\">\r\n                        <el-select v-model=\"formInline.ren\" id=\"tex\" placeholder=\"请输入\">\r\n                            <el-option v-for=\"item in List\" :key=\"item.u_id\" :label=\"item.u_True_name\"\r\n                                :value=\"item.u_id\"></el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item id=\"w\">\r\n                        <el-button @click=\"search()\" type=\"primary\" id=\"tex1\" icon=\"el-icon-search\">搜索</el-button>\r\n                        <el-button @click=\"resetForm()\" id=\"tex2\" icon=\"el-icon-refresh\">重置</el-button>\r\n                        <!-- <el-button type=\"primary\" @click=\"search\">高级搜索</el-button> -->\r\n                    </el-form-item>\r\n                </el-form>\r\n            </div>\r\n        </transition-group>\r\n\r\n        <!--按钮-->\r\n        <div id=\"an\">\r\n            <el-button id=\"tex3\" icon=\"el-icon-plus\" @click=\"handleEdit()\">新增</el-button>\r\n            <!-- <el-button  id=\"tex4\"  icon=\"el-icon-edit\">修改</el-button> -->\r\n            <el-button id=\"tex5\" @click=\"delAll()\" icon=\"el-icon-delete\"\r\n                :disabled=\"multipleSelection.length == 0\">删除</el-button>\r\n            <el-button id=\"tex7\" icon=\"el-icon-upload2\">导入</el-button>\r\n            <el-button id=\"tex6\" icon=\"el-icon-download\">导出</el-button>\r\n        </div>\r\n        <!--隐藏-->\r\n        <div id=\"an1\">\r\n            <el-button icon=\"el-icon-search\" circle @click=\"showDefaultAttr = !showDefaultAttr\" title=\"展开\"></el-button>\r\n            <el-button icon=\"el-icon-refresh\" circle @click=\"flushed\" title=\"刷新\"></el-button>\r\n        </div>\r\n        <!-- 表单 -->\r\n        <el-table tooltip-effect=\"dark\" v-loading=\"tableloading\" @selection-change=\"handleSelectionChange\" :data=\"tableData\"\r\n            :header-cell-style=\"{ background: '#F8F8F9', color: '#606266' }\" id=\"tableMain\">\r\n            <el-table-column type=\"selection\" width=\"55\">\r\n            </el-table-column>\r\n            <el-table-column fixed prop=\"customer_id\" label=\"序号\" width=\"50\">\r\n            </el-table-column>\r\n            <el-table-column fixed prop=\"cust_code\" label=\"客户编码\" width=\"120\">\r\n                <template slot-scope=\"{ row }\">\r\n                    <router-link :to=\"{ name: 'CustomerXiang', query: { dh: row.cust_name} }\"\r\n                        style=\"color: #9c27b0;\">{{\r\n                            row.cust_code\r\n                        }}</router-link>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column fixed prop=\"cust_name\" label=\"客户名称\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"line_type\" label=\"所属行业\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"cust_from\" label=\"客户来源\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"link_man\" label=\"联系人\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"link_tel\" label=\"联系电话\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"e_mail\" label=\"电子邮箱\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"head_of_Sales\" label=\"销售负责人\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"inbill_state\" label=\"状态\" width=\"60\">\r\n                <template slot-scope=\"scope\">\r\n                    <span v-if=\"scope.row.inbill_state == 0\" style=\"color: blue;font-weight: 600;font-size: 13px;\">启用</span>\r\n                    <span v-if=\"scope.row.inbill_state == 1\" style=\"color: red;font-weight: 600;font-size: 13px;\">停用</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"cust_adr\" label=\"客户地址\" width=\"250\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"finance_name\" label=\"账号名称\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"tax_no\" label=\"纳税人识别号\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"bank_no\" label=\"银行账户\" width=\"200\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"open_bank\" label=\"开户银行\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"open_adr\" label=\"开户行地址\" width=\"200\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"operatoro\" label=\"操作人\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"opetime\" label=\"操作时间\" width=\"140\">\r\n                <template slot-scope='scope'>\r\n                            <span>{{ filterTime(scope.row.opetime) }}</span>\r\n                      </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"操作\" fixed=\"right\" width=\"100\">\r\n                <template slot-scope=\"scope\">\r\n                    <img src=\"../Images/bianji1.png\" width=\"25px\" height=\"25px\" title=\"编辑\" @click=\"update(scope.row.customer_id)\">\r\n                    <img src=\"../Images/shanchu.png\" style=\"margin-left: 10px;\" width=\"28px\" height=\"28px\" title=\"删除\" @click=\"handleDelete(scope.row)\">\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n\r\n        <!-- 分页 -->\r\n        <el-pagination id=\"fenye\" @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\"\r\n            :current-page=\"currentPage\" :page-sizes=\"[3, 6, 9, 12]\" :page-size=\"pagesize\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\" :total=\"rows\">\r\n        </el-pagination>\r\n\r\n    </div>\r\n</template>\r\n  \r\n<script>\r\nimport $http from \"../http/base.js\";\r\nvar tj = \"1 = 1 and is_del = '0'\";\r\nexport default {\r\n    data() {\r\n        return {\r\n            tableloading: false,//加载\r\n            showDefaultAttr: true,\r\n            multipleSelection: [],// 选中的行数据\r\n            formInline: {\r\n                kh: '',\r\n                zt: '',\r\n                hy: '',\r\n                ren: '',\r\n            },\r\n            tableData: [],\r\n            List1:[],\r\n            currentPage: 1,//当前页\r\n            pagesize: 3,//每页显示数\r\n            rows: 0,//总行数\r\n            tj,\r\n            List: [],\r\n            str: '1',\r\n            ss:[],\r\n        }\r\n\r\n    },\r\n    methods: {\r\n        //查询方法\r\n        find() {\r\n            var t = this;\r\n            $http\r\n                .post(\r\n                    \"http://localhost:5149/api/CustomerView/FenYeCustomer?currentPage=\" + t.currentPage\r\n                    + \"&pageSize=\" + t.pagesize + \"&str=\" + tj + \"\"\r\n                )\r\n                .then(res => {\r\n                    t.tableData = res.list;\r\n                    t.rows = res.rows;\r\n                    t.lxr();\r\n                    t.hy();\r\n                    console.log(\"----\")\r\n                    console.log(res);\r\n                    console.log(\"----\")\r\n                })\r\n                .catch(err => {\r\n                    alert(err);\r\n                });\r\n        },\r\n        lxr() {\r\n            $http.post(\"http://localhost:5149/api/SupplyView/Czr\")\r\n                .then(res => {\r\n                    this.List = res;\r\n                    console.log(res);\r\n                })\r\n                .catch(err => {\r\n                    alert(err);\r\n                });\r\n        },\r\n        //重置\r\n        resetForm() {\r\n            this.formInline.kh = \"\";\r\n            this.formInline.hy = \"\";\r\n            this.formInline.zt = \"\";\r\n            this.formInline.ren = \"\";\r\n            tj = \"1=1\";\r\n            this.find();\r\n        },\r\n        handleEdit() {\r\n            this.$router.push('customerInsert');\r\n        },\r\n        update(i){\r\n            this.$router.push('customerUpdate?id='+i);\r\n        },\r\n        //搜索\r\n        search() {\r\n            tj = \"1=1\"\r\n            if (this.formInline.kh != \"\") {\r\n                tj += \" and cust_Name like '%\" + this.formInline.kh + \"%' or cust_code like '%\" + this.formInline.kh + \"%'\";\r\n            }\r\n            if (this.formInline.zt != \"\") {\r\n                tj += \" and inbill_state like '%\" + this.formInline.zt + \"%'\";\r\n            }\r\n            if (this.formInline.hy != \"\") {\r\n                tj += \" and line_type like '%\" + this.formInline.hy + \"%'\";\r\n            }\r\n            if (this.formInline.ren != \"\") {\r\n                tj += \" and link_man like '%\" + this.formInline.ren + \"%'\";\r\n            }\r\n            this.find();\r\n        },\r\n        //选择\r\n        handleSelectionChange(val) {\r\n            this.multipleSelection = val;\r\n            console.log(this.multipleSelection);\r\n        },\r\n        //删除\r\n        handleDelete(i) {\r\n            this.$confirm('此操作将永久删除该信息, 是否继续?', '提示', {\r\n                confirmButtonText: '确定',\r\n                cancelButtonText: '取消',\r\n                type: 'warning'\r\n            }).then(() => {\r\n                $http\r\n                    .post(\"http://localhost:5149/api/CustomerView/CustomerDelete?id=\" + i.customer_id + \"&str=\" + this.str)\r\n                    .then(res => {\r\n\r\n                        if (res) {\r\n                            this.$message({\r\n                                message: '删除成功！',\r\n                                type: 'success'\r\n                            });\r\n                            this.find();\r\n                        }\r\n                    })\r\n                    .catch(err => {\r\n                        console.log(err);\r\n                    });\r\n            }).catch(() => {\r\n                this.$message({\r\n                    type: 'error',\r\n                    message: '已取消删除'\r\n                });\r\n            });\r\n        },\r\n        //每页显示数改变事件\r\n        handleSizeChange(size) {\r\n            this.pagesize = size;\r\n            this.currentPage = 1;\r\n            this.find();\r\n            console.log(`每页 ${size} 条`);\r\n        },\r\n        //页码改变事件\r\n        handleCurrentChange(page) {\r\n            this.currentPage = page;\r\n            this.find();\r\n            console.log(`当前页: ${page}`);\r\n        },\r\n        //刷新table\r\n        flushed() {\r\n            this.tableloading = true\r\n            // 模拟获取接口数据\r\n            setTimeout(() => {\r\n                //调用find();this.find();\r\n                this.tableloading = false;\r\n                this.find();\r\n            }, 1000)\r\n            //直接this.查询方法();\r\n        },\r\n        hy() {\r\n            $http.post('http://localhost:5149/api/CustomerView/HY')\r\n                .then(res => {\r\n                    this.List1 = res;\r\n                })\r\n                .catch(err => {\r\n                    alert(err);\r\n                })\r\n        },\r\n        //批量删除\r\n        delAll() {\r\n            this.$confirm('此操作将永久删除该信息, 是否继续?', '提示', {\r\n                confirmButtonText: '确定',\r\n                cancelButtonText: '取消',\r\n                type: 'warning'\r\n            }).then(() => {\r\n                const length = this.multipleSelection.length;\r\n                var ss = [];\r\n                for(let i = 0;i < this.multipleSelection.length;i++){\r\n                    ss.push(this.multipleSelection[i].customer_id);\r\n                }\r\n                console.log('lo'+length)\r\n                $http\r\n                    .put('http://localhost:5149/api/CustomerView/PLDeleteCustomer',ss)\r\n                    .then(res => {\r\n\r\n                        if (res) {\r\n                            this.$message({\r\n                                message: '删除成功！',\r\n            