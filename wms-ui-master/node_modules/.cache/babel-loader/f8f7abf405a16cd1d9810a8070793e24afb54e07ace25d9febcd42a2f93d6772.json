{"ast":null,"code":"import $http from \"../http/base.js\";\nexport function formatDate(date) {\n  let time = new Date(date);\n  console.log(time);\n  let year = time.getFullYear();\n  console.log(year);\n  let month = time.getMonth() + 1;\n  month = month.toString().padStart(2, \"0\");\n  let day = time.getDate().toString().padStart(2, \"0\");\n  return year + \"年\" + month + \"月\" + day + \"日\";\n}\nvar str = \"1=1 and is_del=0\";\nexport default {\n  data() {\n    return {\n      formLabelWidth: \"80px\",\n      drawer: false,\n      direction: \"rtl\",\n      dialogFormVisible: false,\n      tableloading: false,\n      //加载\n      showDefaultAttr: true,\n      multipleSelection: [],\n      // 选中的行数据\n      formInline: {\n        bh: \"\",\n        zt: \"\",\n        gys: \"\",\n        date: \"\"\n      },\n      tableData: [],\n      currentPage: 1,\n      //当前页\n      pagesize: 10,\n      //每页显示数\n      rows: 0,\n      //总行数\n      str,\n      delarr: [],\n      resource: \"2\",\n      desc: \"\",\n      buy_codeId: 0\n    };\n  },\n  methods: {\n    daoru() {},\n    daochu() {},\n    qd() {\n      $http.put(\"http://localhost:5149/api/Wh_buy_order_h/Wh_buy_order_h_sale_state?sale_state=\" + this.resource + \"&sure_man=\" + localStorage.getItem(\"name\") + \"&buy_codeId=\" + this.buy_codeId + \"&sale_state_yj=\" + this.desc + \"\").then(res => {\n        if (res) {\n          this.$message({\n            message: \"审核成功！\",\n            type: \"success\"\n          });\n          this.dialogFormVisible = false;\n          this.find();\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    //取消\n    qx() {\n      this.dialogFormVisible = false;\n    },\n    //撤销\n    chexiao() {\n      if (this.multipleSelection.length == 0) {\n        this.$notify.error({\n          title: \"错误\",\n          message: \"请选择要进行撤销的数据\"\n        });\n        return;\n      }\n      for (let i = 0; i < this.multipleSelection.length; i++) {\n        var zt = this.multipleSelection[i].sale_state;\n      }\n      if (zt == 0) {\n        this.$message({\n          message: \"无法执行该操作\",\n          type: \"warning\"\n        });\n        return;\n      } else if (zt == 1) {\n        this.$message({\n          message: \"无法执行该操作\",\n          type: \"warning\"\n        });\n        return;\n      } else if (zt > 1) {\n        this.$confirm(\"是否撤销所选单据\", \"提示\", {\n          confirmButtonText: \"确定\",\n          cancelButtonText: \"取消\",\n          type: \"warning\"\n        }).then(() => {\n          $http.put(\"http://localhost:5149/api/Wh_buy_order_h/Wh_buy_order_h_chexiao?sure_man=\" + localStorage.getItem(\"name\") + \"&buy_codeId=\" + this.buy_codeId + \"\").then(res => {\n            if (res) {\n              this.$message({\n                message: \"撤销成功!\",\n                type: \"success\"\n              });\n              this.find();\n            }\n          }).catch(err => {\n            console.log(err);\n          });\n        }).catch(() => {\n          this.$message({\n            type: \"error\",\n            message: \"已取消撤销\"\n          });\n        });\n      }\n    },\n    //审核\n    sh() {\n      if (this.multipleSelection.length == 0) {\n        this.$notify.error({\n          title: \"错误\",\n          message: \"请选择要进行审核的数据\"\n        });\n        return;\n      }\n      for (let i = 0; i < this.multipleSelection.length; i++) {\n        var zt = this.multipleSelection[i].sale_state;\n      }\n      if (zt == 0) {\n        this.dialogFormVisible = true;\n      } else if (zt == 1) {\n        this.$message({\n          message: \"无法执行该操作\",\n          type: \"warning\"\n        });\n        return;\n      } else if (zt >= 2) {\n        this.$message({\n          message: \"无法执行该操作\",\n          type: \"warning\"\n        });\n        return;\n      }\n    },\n    jhdh(id) {\n      this.$router.push({\n        path: \"/IncomingOrdersCode\",\n        query: {\n          buy_code: id\n        }\n      });\n    },\n    handleEdit(id) {\n      this.$router.push({\n        path: \"/IncomingOrdersUpdate\",\n        query: {\n          buy_code: id\n        }\n      });\n      // this.drawer = true;\n    },\n\n    handleClose(done) {\n      this.$confirm(\"确认关闭？\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(_ => {\n        done();\n      }).catch(_ => {});\n    },\n    inserok() {\n      this.$router.push(\"/IncomingOrdersAdd\");\n    },\n    //查询方法\n    find() {\n      var t = this;\n      $http.get(\"http://localhost:5149/api/Wh_buy_order_h/Wh_buy_order_hFind\", {\n        params: {\n          pageSize: t.pagesize,\n          currentPage: t.currentPage,\n          str\n        }\n      }).then(res => {\n        console.log(res);\n        t.tableData = res.list;\n        t.rows = res.rows;\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    //重置\n    reset() {\n      this.formInline = {\n        bh: \"\",\n        zt: \"\",\n        gys: \"\",\n        date: \"\"\n      };\n    },\n    //搜索\n    search() {\n      this.currentPage = 1;\n      str = \"1=1 and is_del=0  \";\n      if (this.formInline.bh != \"\") {\n        str += \"and buy_code like '%\" + this.formInline.bh + \"%'\";\n      }\n      if (this.formInline.zt != \"\") {\n        if (this.formInline.zt == 2) {\n          str += \"and sale_state =2 or sale_state =3 or sale_state =4\";\n        } else {\n          str += \"and sale_state like '%\" + this.formInline.zt + \"%'\";\n        }\n      }\n      if (this.formInline.gys != \"\") {\n        str += \"and supply_code like '%\" + this.formInline.gys + \"%'\";\n      }\n      if (this.formInline.date != \"\") {\n        const yearMonthDay = this.formInline.date.substring(0, 10);\n        str += \"and convert(varchar(50),buy_date,120) LIKE  '\" + yearMonthDay + \"%'\";\n      }\n      this.find();\n    },\n    //选择\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n      for (let i = 0; i < this.multipleSelection.length; i++) {\n        this.buy_codeId = this.multipleSelection[i].buy_codeId;\n      }\n      console.log(this.multipleSelection);\n    },\n    //删除\n    handleDelete(buy_codeId) {\n      this.$confirm(\"此操作将永久删除该信息, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        $http.put(\"http://localhost:5149/api/Wh_buy_order_h/Wh_buy_order_h_Delete?buy_codeId=\" + buy_codeId + \"\").then(res => {\n          if (res) {\n            this.$message({\n              message: \"删除成功!\",\n              type: \"success\"\n            });\n            this.find();\n          }\n        }).catch(err => {\n          console.log(err);\n        });\n      }).catch(() => {\n        this.$message({\n          type: \"error\",\n          message: \"已取消删除\"\n        });\n      });\n    },\n    //每页显示数改变事件\n    handleSizeChange(size) {\n      this.pagesize = size;\n      this.currentPage = 1;\n      this.find();\n    },\n    //页码改变事件\n    handleCurrentChange(page) {\n      this.currentPage = page;\n      this.find();\n    },\n    //刷新table\n    flushed() {\n      this.tableloading = true;\n      // 模拟获取接口数据\n      setTimeout(() => {\n        //调用find();\n        this.find();\n        this.tableloading = false;\n      }, 1000);\n      //直接this.查询方法();\n    },\n\n    //批量删除\n    delAll() {\n      this.$confirm(\"此操作将永久删除该信息, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        const length = this.multipleSelection.length;\n        var strs = [];\n        for (let i = 0; i < this.multipleSelection.length; i++) {\n          strs.push(this.multipleSelection[i].buy_codeId);\n        }\n        console.log(strs);\n        $http.put(\"http://localhost:5149/api/Wh_buy_order_h/Wh_buy_order_h_DeleteArry/route\", strs).then(res => {\n          if (res) {\n            this.$message({\n              message: \"删除成功!\",\n              type: \"success\"\n            });\n            this.find();\n          }\n        }).catch(err => {\n          console.log(err);\n        });\n      }).catch(() => {\n        this.$message({\n          type: \"error\",\n          message: \"已取消删除\"\n        });\n      });\n    },\n    //时间格式化\n    filterTime(timestamp) {\n      return formatDate(timestamp);\n    },\n    cancelForm() {\n      this.drawer = false;\n    }\n  },\n  mounted() {\n    this.tableloading = true;\n    // 模拟获取接口数据\n    setTimeout(() => {\n      //调用find();\n      this.find();\n      this.tableloading = false;\n    }, 500);\n  }\n};","map":{"version":3,"names":["$http","formatDate","date","time","Date","console","log","year","getFullYear","month","getMonth","toString","padStart","day","getDate","str","data","formLabelWidth","drawer","direction","dialogFormVisible","tableloading","showDefaultAttr","multipleSelection","formInline","bh","zt","gys","tableData","currentPage","pagesize","rows","delarr","resource","desc","buy_codeId","methods","daoru","daochu","qd","put","localStorage","getItem","then","res","$message","message","type","find","catch","err","qx","chexiao","length","$notify","error","title","i","sale_state","$confirm","confirmButtonText","cancelButtonText","sh","jhdh","id","$router","push","path","query","buy_code","handleEdit","handleClose","done","_","inserok","t","get","params","pageSize","list","reset","search","yearMonthDay","substring","handleSelectionChange","val","handleDelete","handleSizeChange","size","handleCurrentChange","page","flushed","setTimeout","delAll","strs","filterTime","timestamp","cancelForm","mounted"],"sources":["src/components/IncomingOrders.vue"],"sourcesContent":["<template>\r\n  <div id=\"kj\">\r\n    <!--头部-->\r\n    <el-breadcrumb separator=\"/\" id=\"card\">\r\n      <el-breadcrumb-item :to=\"{ path: '/Home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item\r\n        ><a href=\"javascript:void(0)\">订单管理</a></el-breadcrumb-item\r\n      >\r\n      <el-breadcrumb-item>进货订单</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <!--搜索-->\r\n    <transition-group name=\"lyric\">\r\n      <div id=\"form\" v-if=\"showDefaultAttr\" key=\"1\">\r\n        <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n          <el-form-item label=\"进货订单\" id=\"form_top\">\r\n            <el-input\r\n              v-model=\"formInline.bh\"\r\n              id=\"tex\"\r\n              placeholder=\"进货订单\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"单据状态\" id=\"form_top\">\r\n            <el-select v-model=\"formInline.zt\" id=\"tex\" placeholder=\"单据状态\">\r\n              <el-option label=\"待审核\" value=\"0\"></el-option>\r\n              <el-option label=\"驳回\" value=\"1\"></el-option>\r\n              <el-option label=\"通过\" value=\"2\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"供应商\" id=\"form_top\">\r\n            <el-input\r\n              v-model=\"formInline.gys\"\r\n              id=\"tex\"\r\n              placeholder=\"供应商\"\r\n            ></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"进货日期\" prop=\"startDate\">\r\n            <el-date-picker\r\n              v-model=\"formInline.date\"\r\n              type=\"datetime\"\r\n              id=\"tex\"\r\n              placeholder=\"选择日期时间\"\r\n              value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n            >\r\n            </el-date-picker>\r\n          </el-form-item>\r\n          <el-form-item id=\"w\">\r\n            <el-button\r\n              @click=\"search()\"\r\n              type=\"primary\"\r\n              id=\"tex1\"\r\n              icon=\"el-icon-search\"\r\n              >搜索</el-button\r\n            >\r\n            <el-button @click=\"reset()\" id=\"tex2\" icon=\"el-icon-refresh\"\r\n              >重置</el-button\r\n            >\r\n            <!-- <el-button type=\"primary\" @click=\"search\">高级搜索</el-button> -->\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n    </transition-group>\r\n\r\n    <!--按钮-->\r\n    <div id=\"an\">\r\n      <el-button id=\"tex3\" @click=\"inserok()\" icon=\"el-icon-plus\"\r\n        >新增</el-button\r\n      >\r\n      <!-- <el-button  id=\"tex4\"  icon=\"el-icon-edit\">修改</el-button> -->\r\n      <el-button\r\n        id=\"tex5\"\r\n        @click=\"delAll()\"\r\n        icon=\"el-icon-delete\"\r\n        :disabled=\"multipleSelection.length == 0\"\r\n        >删除</el-button\r\n      >\r\n      <el-button\r\n        id=\"tex7\"\r\n        :disabled=\"multipleSelection.length > 1\"\r\n        @click=\"sh()\"\r\n        icon=\"el-icon-setting\"\r\n        >审核</el-button\r\n      >\r\n      <el-button\r\n        id=\"tex6\"\r\n        @click=\"chexiao\"\r\n        :disabled=\"multipleSelection.length > 1\"\r\n        icon=\"el-icon-sort\"\r\n        >撤销</el-button\r\n      >\r\n    </div>\r\n    <!--隐藏-->\r\n    <div id=\"an1\">\r\n      <el-button\r\n        icon=\"el-icon-upload\"\r\n        circle\r\n        @click=\"daoru\"\r\n        title=\"导入\"\r\n      ></el-button>\r\n      <el-button\r\n        icon=\"el-icon-download\"\r\n        circle\r\n        @click=\"daochu\"\r\n        title=\"导出\"\r\n      ></el-button>\r\n      <el-button\r\n        icon=\"el-icon-search\"\r\n        circle\r\n        @click=\"showDefaultAttr = !showDefaultAttr\"\r\n        title=\"展开\"\r\n      ></el-button>\r\n      <el-button\r\n        icon=\"el-icon-refresh\"\r\n        circle\r\n        @click=\"flushed\"\r\n        title=\"刷新\"\r\n      ></el-button>\r\n    </div>\r\n    <!-- 表单 -->\r\n    <el-table\r\n      tooltip-effect=\"dark\"\r\n      v-loading=\"tableloading\"\r\n      height=\"500px\"\r\n      @selection-change=\"handleSelectionChange\"\r\n      :data=\"tableData\"\r\n      :header-cell-style=\"{ background: '#F8F8F9', color: '#606266' }\"\r\n      element-loading-text=\"拼命加载中\"\r\n      element-loading-spinner=\"el-icon-loading\"\r\n      id=\"tableMain\"\r\n    >\r\n      <el-table-column type=\"selection\" width=\"55\"> </el-table-column>\r\n      <el-table-column fixed prop=\"\" label=\"进货单号\" width=\"150\">\r\n        <template slot-scope=\"scope\">\r\n          <a\r\n            href=\"javascript:void(0)\"\r\n            style=\"color: #9c27b0\"\r\n            v-on:click=\"jhdh(scope.row.buy_code)\"\r\n            >{{ scope.row.buy_code }}</a\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column fixed label=\"单据状态\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <span\r\n            v-if=\"scope.row.sale_state == 0\"\r\n            style=\"color: blue; font-weight: 600; font-size: 13px\"\r\n            >待审核</span\r\n          >\r\n          <span\r\n            v-if=\"scope.row.sale_state == 1\"\r\n            style=\"color: red; font-weight: 600; font-size: 13px\"\r\n            >驳回</span\r\n          >\r\n          <span\r\n            v-if=\"scope.row.sale_state > 1\"\r\n            style=\"color: Green; font-weight: 600; font-size: 13px\"\r\n            >通过</span\r\n          >\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column fixed label=\"进货日期\" width=\"150\">\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ filterTime(scope.row.buy_date) }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"supply_code\" label=\"供应商\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"link_man\" label=\"联系人\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"link_way\" label=\"联系方式\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"buy_man\" label=\"进货人\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"total_buy_num\" label=\"货品数量\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"total_price\" label=\"货品金额\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"names\"\r\n        :show-overflow-tooltip=\"true\"\r\n        label=\"进货货品\"\r\n        width=\"120\"\r\n      >\r\n      </el-table-column>\r\n      <el-table-column prop=\"create_by\" label=\"制单人\" width=\"120\">\r\n      </el-table-column>\r\n      <el-table-column label=\"制单时间\" width=\"150\">\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ filterTime(scope.row.create_time) }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"sure_man\" label=\"审核人\" width=\"120\">\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"审核时间\" width=\"150\">\r\n        <template slot-scope=\"scope\">\r\n          <span v-if=\"scope.row.sure_date != '0001-01-01T00:00:00'\">{{\r\n            filterTime(scope.row.sure_date)\r\n          }}</span>\r\n          <span v-else></span>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"操作\" fixed=\"right\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <span v-if=\"scope.row.sale_state == 2\"> </span>\r\n          <span v-if=\"scope.row.sale_state == 1 || scope.row.sale_state == 0\">\r\n            <img\r\n              src=\"../Images/bianji1.png\"\r\n              title=\"编辑\"\r\n              width=\"25px\"\r\n              height=\"25px\"\r\n              style=\"position: relative; left: 0px; top: 5px\"\r\n              @click=\"handleEdit(scope.row.buy_code)\"\r\n            />\r\n          </span>\r\n          <img\r\n            src=\"../Images/shanchu.png\"\r\n            title=\"删除\"\r\n            style=\"position: absolute; left: 40px; top: 15px\"\r\n            width=\"28px\"\r\n            height=\"28px\"\r\n            @click=\"handleDelete(scope.row.buy_codeId)\"\r\n          />\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      id=\"fenye\"\r\n      @size-change=\"handleSizeChange\"\r\n      @current-change=\"handleCurrentChange\"\r\n      :current-page=\"currentPage\"\r\n      :page-sizes=\"[10, 20, 30, 40]\"\r\n      :page-size=\"pagesize\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\"\r\n      :total=\"rows\"\r\n    >\r\n    </el-pagination>\r\n\r\n    <el-drawer\r\n      title=\"新增\"\r\n      :visible.sync=\"drawer\"\r\n      :direction=\"direction\"\r\n      :before-close=\"handleClose\"\r\n    >\r\n      <div class=\"demo-drawer__content\">\r\n        <el-form :model=\"formInline\">\r\n          <el-form-item label=\"活动名称\" :label-width=\"formLabelWidth\">\r\n            <el-input v-model=\"formInline.bh\" autocomplete=\"off\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"活动区域\" :label-width=\"formLabelWidth\">\r\n            <el-select v-model=\"formInline.bh\" placeholder=\"请选择活动区域\">\r\n              <el-option label=\"区域一\" value=\"shanghai\"></el-option>\r\n              <el-option label=\"区域二\" value=\"beijing\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-form>\r\n        <div class=\"demo-drawer__footer\" id=\"didi\">\r\n          <el-button id=\"tj\" @click=\"closeDrawer()\">提交</el-button>\r\n          <el-button @click=\"cancelForm\" id=\"qx\">取 消</el-button>\r\n        </div>\r\n      </div>\r\n    </el-drawer>\r\n\r\n    <el-dialog\r\n      title=\"审核\"\r\n      :visible.sync=\"dialogFormVisible\"\r\n      width=\"430px\"\r\n      id=\"tjshh\"\r\n    >\r\n      <el-form style=\"height: 225px\">\r\n        <el-form-item label=\"审核结果\">\r\n          <el-radio-group v-model=\"resource\" id=\"dx\">\r\n            <el-radio v-model=\"resource\" label=\"2\" value=\"2\">通过</el-radio>\r\n            <el-radio label=\"1\" value=\"1\">驳回</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        <el-form-item label=\"审核意见\">\r\n          <el-input type=\"textarea\" id=\"yj\" v-model=\"desc\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button id=\"gb1\" @click=\"qx()\">取 消</el-button>\r\n        <el-button\r\n          id=\"bc12\"\r\n          style=\"background-color: #9c27b0; color: #fff\"\r\n          @click=\"qd()\"\r\n          >确 定</el-button\r\n        >\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport $http from \"../http/base.js\";\r\nexport function formatDate(date) {\r\n  let time = new Date(date);\r\n  console.log(time);\r\n  let year = time.getFullYear();\r\n  console.log(year);\r\n  let month = time.getMonth() + 1;\r\n  month = month.toString().padStart(2, \"0\");\r\n  let day = time.getDate().toString().padStart(2, \"0\");\r\n  return year + \"年\" + month + \"月\" + day + \"日\";\r\n}\r\nvar str = \"1=1 and is_del=0\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      formLabelWidth: \"80px\",\r\n      drawer: false,\r\n      direction: \"rtl\",\r\n      dialogFormVisible: false,\r\n      tableloading: false, //加载\r\n      showDefaultAttr: true,\r\n      multipleSelection: [], // 选中的行数据\r\n      formInline: {\r\n        bh: \"\",\r\n        zt: \"\",\r\n        gys: \"\",\r\n        date: \"\",\r\n      },\r\n      tableData: [],\r\n      currentPage: 1, //当前页\r\n      pagesize: 10, //每页显示数\r\n      rows: 0, //总行数\r\n      str,\r\n      delarr: [],\r\n      resource: \"2\",\r\n      desc: \"\",\r\n      buy_codeId: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    daoru() {},\r\n    daochu() {},\r\n    qd() {\r\n      $http\r\n        .put(\r\n          \"http://localhost:5149/api/Wh_buy_order_h/Wh_buy_order_h_sale_state?sale_state=\" +\r\n            this.resource +\r\n            \"&sure_man=\" +\r\n            localStorage.getItem(\"name\") +\r\n            \"&buy_codeId=\" +\r\n            this.buy_codeId +\r\n            \"&sale_state_yj=\" +\r\n            this.desc +\r\n            \"\"\r\n        )\r\n        .then((res) => {\r\n          if (res) {\r\n            this.$message({\r\n              message: \"审核成功！\",\r\n              type: \"success\",\r\n            });\r\n            this.dialogFormVisible = false;\r\n            this.find();\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    //取消\r\n    qx() {\r\n      this.dialogFormVisible = false;\r\n    },\r\n    //撤销\r\n    chexiao() {\r\n      if (this.multipleSelection.length == 0) {\r\n        this.$notify.error({\r\n          title: \"错误\",\r\n          message: \"请选择要进行撤销的数据\",\r\n        });\r\n        return;\r\n      }\r\n      for (let i = 0; i < this.multipleSelection.length; i++) {\r\n        var zt = this.multipleSelection[i].sale_state;\r\n      }\r\n      if (zt == 0) {\r\n        this.$message({\r\n          message: \"无法执行该操作\",\r\n          type: \"warning\",\r\n        });\r\n        return;\r\n      } else if (zt == 1) {\r\n        this.$message({\r\n          message: \"无法执行该操作\",\r\n          type: \"warning\",\r\n        });\r\n        return;\r\n      } else if (zt > 1) {\r\n        this.$confirm(\"是否撤销所选单据\", \"提示\", {\r\n          confirmButtonText: \"确定\",\r\n          cancelButtonText: \"取消\",\r\n          type: \"warning\",\r\n        })\r\n          .then(() => {\r\n            $http\r\n              .put(\r\n                \"http://localhost:5149/api/Wh_buy_order_h/Wh_buy_order_h_chexiao?sure_man=\" +\r\n                  localStorage.getItem(\"name\") +\r\n                  \"&buy_codeId=\" +\r\n                  this.buy_codeId +\r\n                  \"\"\r\n              )\r\n              .then((res) => {\r\n                if (res) {\r\n                  this.$message({\r\n                    message: \"撤销成功!\",\r\n                    type: \"success\",\r\n                  });\r\n                  this.find();\r\n                }\r\n              })\r\n              .catch((err) => {\r\n                console.log(err);\r\n              });\r\n          })\r\n          .catch(() => {\r\n            this.$message({\r\n              type: \"error\",\r\n              message: \"已取消撤销\",\r\n            });\r\n          });\r\n      }\r\n    },\r\n    //审核\r\n    sh() {\r\n      if (this.multipleSelection.length == 0) {\r\n        this.$notify.error({\r\n          title: \"错误\",\r\n          message: \"请选择要进行审核的数据\",\r\n        });\r\n        return;\r\n      }\r\n      for (let i = 0; i < this.multipleSelection.length; i++) {\r\n        var zt = this.multipleSelection[i].sale_state;\r\n      }\r\n      if (zt == 0) {\r\n        this.dialogFormVisible = true;\r\n      } else if (zt == 1) {\r\n        this.$message({\r\n          message: \"无法执行该操作\",\r\n          type: \"warning\",\r\n        });\r\n        return;\r\n      } else if (zt >= 2) {\r\n        this.$message({\r\n          message: \"无法执行该操作\",\r\n          type: \"warning\",\r\n        });\r\n        return;\r\n      }\r\n    },\r\n    jhdh(id) {\r\n      this.$router.push({\r\n        path: \"/IncomingOrdersCode\",\r\n        query: {\r\n          buy_code: id,\r\n        },\r\n      });\r\n    },\r\n    handleEdit(id) {\r\n      this.$router.push({\r\n        path: \"/IncomingOrdersUpdate\",\r\n        query: {\r\n          buy_code: id,\r\n        },\r\n      });\r\n      // this.drawer = true;\r\n    },\r\n    handleClose(done) {\r\n      this.$confirm(\"确认关闭？\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then((_) => {\r\n          done();\r\n        })\r\n        .catch((_) => {});\r\n    },\r\n    inserok() {\r\n      this.$router.push(\"/IncomingOrdersAdd\");\r\n    },\r\n    //查询方法\r\n    find() {\r\n      var t = this;\r\n      $http\r\n        .get(\"http://localhost:5149/api/Wh_buy