{"ast":null,"code":"import $http from \"../http/base.js\";\nvar str = \"1=1 and is_del=0\";\nexport default {\n  data() {\n    return {\n      List1: [],\n      List: [],\n      List2: [],\n      formLabelWidth: \"150px\",\n      formtable: {\n        bh: \"\",\n        dw: \"\"\n      },\n      radio: \"0\",\n      tableData: [],\n      formInline: {\n        supply_code: \"\",\n        supply_name: \"\",\n        e_mail: \"\",\n        supply_type_code: \"\",\n        stock_duty_man: \"\",\n        link_tel: \"\",\n        supply_adr: \"\",\n        link_man: \"\",\n        line_type: \"\",\n        remark: \"\"\n      },\n      formInline1: {\n        finance_name: \"\",\n        tax_no: \"\",\n        bank_no: \"\",\n        open_bank: \"\",\n        open_adr: \"\"\n      },\n      rules: {\n        supply_code: [{\n          required: true,\n          message: \"请填写供应商编码!\",\n          trigger: \"blur\"\n        }],\n        supply_name: [{\n          required: true,\n          message: \"请填写供应商名称!\",\n          trigger: \"blur\"\n        }]\n      }\n    };\n  },\n  methods: {\n    //保存\n    bc() {\n      this.$refs[\"ruleForm1\"].validate(valid => {\n        if (!valid) return;\n        var t = this;\n        var uu = localStorage.getItem(\"name\");\n        $http.get(\"http://localhost:5149/api/SupplyView/SupplyName?name=\" + t.formInline.supply_name).then(res => {\n          if (res == 0) {\n            $http.post(\"http://localhost:5149/api/SupplyView/FinanceInsert\", {\n              finance_name: t.formInline1.finance_name,\n              tax_no: t.formInline1.tax_no,\n              bank_no: t.formInline1.bank_no,\n              open_bank: t.formInline1.open_bank,\n              open_adr: t.formInline1.open_adr,\n              finance_type: \"\"\n            }).then(res => {\n              if (res) {\n                $http.post(\"http://localhost:5149/api/SupplyView/SupplyInsert?code=\" + t.formInline.supply_code + \"&name=\" + t.formInline.supply_name + \"&type=\" + t.formInline.line_type + \"&typecode=\" + t.formInline.supply_type_code + \"&man=\" + t.formInline.link_man + \"&tel=\" + t.formInline.link_tel + \"&yx=\" + t.formInline.e_mail + \"&sdman=\" + t.formInline.stock_duty_man + \"&zt=\" + t.radio + \"&adr=\" + t.formInline.supply_adr + \"&cz=\" + uu + \"&bz=\" + t.formInline.remark + \"\")\n                // $http.post(\"http://localhost:5149/api/SupplyView/SupplyInsert\", {\n                //     code: t.formInline.supply_code,\n                //     supply_name: t.formInline.supply_name,\n                //     line_type: t.formInline.line_type,\n                //     supply_type_code: t.formInline.supply_type_code,\n                //     link_man: t.formInline.link_man,\n                //     link_tel: t.formInline.link_tel,\n                //     e_mail: t.formInline.e_mail,\n                //     stock_duty_man: t.formInline.stock_duty_man,\n                //     inbill_state: t.radio,\n                //     supply_adr: t.formInline.supply_adr,\n                //     finance_id:\"\",\n                //     operatoro: uu,\n                //     opetime:\"\",\n                //     is_del:\"\",\n                //     remark: t.formInline.remark\n                // })\n                .then(res => {\n                  console.log(res);\n                  if (res) {\n                    this.$message({\n                      message: \"添加成功！\",\n                      type: \"success\"\n                    });\n                    this.$router.push(\"/Supplier\");\n                    t.formInline.supply_code = \"\";\n                    t.formInline.supply_name = \"\";\n                    t.formInline.line_type = \"\";\n                    t.formInline.supply_type_code = \"\";\n                    t.formInline.link_man = \"\";\n                    t.formInline.link_tel = \"\";\n                    t.formInline.e_mail = \"\";\n                    t.formInline.stock_duty_man = \"\";\n                    t.radio = 0;\n                    t.formInline.supply_adr = \"\";\n                    t.formInline.remark = \"\";\n                    t.formInline1.finance_name = \"\";\n                    t.formInline1.tax_no = \"\";\n                    t.formInline1.bank_no = \"\";\n                    t.formInline1.open_bank = \"\";\n                    t.formInline1.open_adr = \"\";\n                  }\n                }).catch(err => {\n                  console.log(err);\n                });\n              }\n            }).catch(err => {\n              console.log(err);\n            });\n          } else {\n            this.$message({\n              message: \"供应商名称已存在！\",\n              type: \"error\"\n            });\n          }\n        }).catch(err => {\n          console.log(err);\n        });\n      });\n    },\n    yh() {},\n    Dang() {\n      $http.get(\"http://localhost:5149/api/Sys_encode/FindSys_encodeById?id=\" + 18).then(res => {\n        this.formInline.supply_code = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //关闭\n    gb() {\n      this.$router.go(-1);\n    },\n    fzr() {\n      $http.post(\"http://localhost:5149/api/SupplyView/Czr\").then(res => {\n        this.List = res;\n        console.log(res);\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    fl() {\n      $http.post(\"http://localhost:5149/api/SupplyView/FL\").then(res => {\n        this.List2 = res;\n        console.log(res);\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    hy() {\n      $http.post(\"http://localhost:5149/api/SupplyView/HY\").then(res => {\n        this.List1 = res;\n      }).catch(err => {\n        alert(err);\n      });\n    }\n  },\n  mounted() {\n    this.formInline.create_by = localStorage.getItem(\"name\");\n    this.fzr();\n    this.hy();\n    this.fl();\n    this.Dang();\n  }\n};","map":{"version":3,"names":["$http","str","data","List1","List","List2","formLabelWidth","formtable"