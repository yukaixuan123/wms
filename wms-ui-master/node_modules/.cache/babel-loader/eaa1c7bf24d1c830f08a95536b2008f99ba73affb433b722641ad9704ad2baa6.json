{"ast":null,"code":"import $http from \"../http/base.js\";\nexport function formatDate(date) {\n  let time = new Date(date);\n  let year = time.getFullYear();\n  let month = time.getMonth() + 1;\n  month = month.toString().padStart(2, \"0\");\n  let day = time.getDate().toString().padStart(2, \"0\");\n  return year + \"年\" + month + \"月\" + day + \"日\";\n}\nexport default {\n  data() {\n    return {\n      countryList: [],\n      countryList2: [],\n      formInline: {\n        hpname: \"\",\n        hptype: \"\",\n        lx: \"\",\n        ckname: \"\",\n        rq: \"\"\n      },\n      ff: {\n        name: \"\",\n        bh: \"\",\n        lx: \"\",\n        gg: \"\",\n        dw: \"\"\n      },\n      tableData: [],\n      multipleSelection: [],\n      str: \"1=1\",\n      currentPage: 1,\n      //当前页\n      pagesize: 600,\n      //每页显示数\n      rows: 0 //总行数\n    };\n  },\n\n  methods: {\n    gb() {\n      this.$router.go(-1);\n    },\n    //保留两位小数好\n    formatMoney(value) {\n      return value.toFixed(2);\n    },\n    //仓库下拉框\n    CK() {\n      var t = this;\n      $http.get(\"http://localhost:5149/api/Bs_warehouse/Bs_warehouseSelect1\").then(res => {\n        t.countryList = res;\n      }).catch(err => {\n        alert(err);\n      });\n      $http.get(\"http://localhost:5149/api/Wh_transfer_h/CRLX\").then(res => {\n        t.countryList2 = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //查询\n    find() {\n      var bh = this.$route.query.bh;\n      $http.get(\"http://localhost:5149/api/Wh_transfer_h/KCX?bh=\" + bh).then(res => {\n        this.ff.name = res.item_name;\n        this.ff.bh = res.item_code;\n        this.ff.lx = res.item_type_name;\n        this.ff.dw = res.unit;\n        this.ff.gg = res.model_code;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //每页显示数改变事件\n    handleSizeChange(size) {\n      this.pagesize = size;\n      this.currentPage = 1;\n      this.find1();\n    },\n    //页码改变事件\n    handleCurrentChange(page) {\n      this.currentPage = page;\n      this.find1();\n    },\n    find1() {\n      var bh = this.$route.query.bh;\n      this.str += \"and item_code='\" + bh + \"'\";\n      $http.post(\"http://localhost:5149/api/Wh_transfer_h/LiuFen?page=\" + this.pagesize + \"&pageSize=\" + this.currentPage + \"&str=\" + this.str).then(res => {\n        this.tableData = res.list;\n        this.rows = res.rows;\n        var ru = 0;\n        var rujin = 0;\n        var cu = 0;\n        var cujin = 0;\n        for (let i = 0; i < this.tableData.length; i++) {\n          ru += this.tableData[i].inbill_num;\n          rujin += this.tableData[i].zong;\n          cu += this.tableData[i].outbill_num;\n          cujin += this.tableData[i].czong;\n        }\n        document.getElementById(\"ru\").innerHTML = ru;\n        document.getElementById(\"rujin\").innerHTML = rujin;\n        document.getElementById(\"cu\").innerHTML = cu;\n        document.getElementById(\"cujin\").innerHTML = cujin;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //选择\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n    },\n    //搜索\n    search() {\n      this.str = \"item_code='\" + this.ff.bh + \"'\";\n      if (this.formInline.hpname != \"\") {\n        this.str += \"and inbill_bh = '\" + this.formtable1.hpname + \"'\";\n      }\n      if (this.formInline.ckname != \"\") {\n        this.str += \" AND ware_name like CONCAT('%','\" + this.formInline.ckname + \"','%') \";\n      }\n      if (this.formInline.hptype != \"\") {\n        if (this.formInline.hptype == \"入库\") {\n          this.str += \" AND out1 is NULL \";\n        }\n        if (this.formInline.hptype == \"出库\") {\n          this.str += \" AND inbill_code is NULL \";\n        }\n      }\n      if (this.formInline.lx != \"\") {\n        this.str += \" AND inbill_type =  '\" + this.formInline.lx + \"'\";\n      }\n      if (this.formInline.rq != \"\" && this.formInline.rq != null) {\n        const yearMonthDay = this.formInline.rq.substring(0, 10);\n        this.str += \"and convert(varchar(50),create_time,120) LIKE  '\" + yearMonthDay + \"%'\";\n      }\n      this.find1();\n    },\n    filterTime(timestamp) {\n      return formatDate(timestamp);\n    },\n    //重置\n    reset() {\n      this.formInline = {\n        hpname: \"\",\n        hptype: \"\",\n        dw: \"\",\n        ckname: \"\",\n        rq: \"\"\n      };\n    }\n  },\n  mounted() {\n    this.find1();\n    this.find();\n    this.CK();\n  }\n};","map":{"version":3,"names":["$http","formatDate","date","time","Date","year","getFullYear","month","getMonth","toString","padStart","day","getDate","data","countryList","countryList2","formInline","hpname","hptype","lx","ckname","rq","ff","name","bh","gg","dw","tableData","multipleSelection","str","currentPage","pagesize","rows","methods","gb","$router","go","formatMoney","value","toFixed","CK","t","get","then","res","catch","err","alert","find","$route","query","item_name","item_code","item_type_name","unit","model_code","handleSizeChange","size","find1","handleCurrentChange","page","post","list","ru","rujin","cu","cujin","i","length","inbill_num","zong","outbill_num","czong","document","getElementById","innerHTML","handleSelectionChange","val","search","formtable1","yearMonthDay","substring","filterTime","timestamp","reset","mounted"],"sources":["src/components/StockInquireCode1.vue"],"sourcesContent":["<template>\r\n  <div id=\"kjj\">\r\n    <!--头部-->\r\n    <div style=\"width: 1138px; height: 55px; border-bottom: 1px solid #eeeeee\">\r\n      <ul id=\"kcfbb\">\r\n        <li>{{ ff.name }}</li>\r\n        <li>{{ ff.bh }}</li>\r\n        <li>\r\n          货品类型<span class=\"lll\">{{ ff.lx }}</span>\r\n        </li>\r\n        <li>\r\n          规格型号<span class=\"lll\">{{ ff.gg }}</span>\r\n        </li>\r\n        <li>\r\n          单位<span class=\"lll\">{{ ff.dw }}</span>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div id=\"form\">\r\n      <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n        <el-form-item label=\"出入库单号\" id=\"form_top\">\r\n          <el-input\r\n            v-model=\"formInline.hpname\"\r\n            id=\"tex\"\r\n            placeholder=\"请输入出库单号/入库单号\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"业务类型\" id=\"form_top\">\r\n          <el-select\r\n            v-model=\"formInline.hptype\"\r\n            clearable\r\n            filterable\r\n            placeholder=\"请选择单位\"\r\n            id=\"bddd\"\r\n          >\r\n            <el-option label=\"入库\" value=\"入库\"></el-option>\r\n            <el-option label=\"出库\" value=\"出库\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"仓库名称\" id=\"form_top\">\r\n          <el-select\r\n            v-model=\"formInline.ckname\"\r\n            clearable\r\n            filterable\r\n            placeholder=\"请选择仓库名称\"\r\n            id=\"bddd\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in countryList\"\r\n              :key=\"item.warehouse_id\"\r\n              :label=\"item.ware_name\"\r\n              :value=\"item.ware_name\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"出入库类型\" id=\"form_top\">\r\n          <el-select\r\n            v-model=\"formInline.lx\"\r\n            clearable\r\n            filterable\r\n            placeholder=\"请选择出入库类型\"\r\n            id=\"bddd\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in countryList2\"\r\n              :key=\"item\"\r\n              :label=\"item\"\r\n              :value=\"item\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"制单日期\">\r\n          <el-date-picker\r\n            v-model=\"formInline.rq\"\r\n            id=\"bddd\"\r\n            type=\"datetime\"\r\n            placeholder=\"制单日期\"\r\n            value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n          ></el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item id=\"w\">\r\n          <el-button\r\n            @click=\"search()\"\r\n            type=\"primary\"\r\n            id=\"tex1\"\r\n            icon=\"el-icon-search\"\r\n            >搜索</el-button\r\n          >\r\n          <el-button @click=\"reset()\" id=\"tex2\" icon=\"el-icon-refresh\"\r\n            >重置</el-button\r\n          >\r\n          <!-- <el-button type=\"primary\" @click=\"search\">高级搜索</el-button> -->\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n\r\n    <div id=\"sandd1\">\r\n      <ul id=\"kcsll1\">\r\n        <li>\r\n          <span\r\n            style=\"font-size: 27px; font-weight: 700; color: #9031aa\"\r\n            id=\"ru\"\r\n          ></span>\r\n        </li>\r\n        <li>入库数量</li>\r\n      </ul>\r\n      <ul id=\"kcslll1\">\r\n        <li>\r\n          <span\r\n            style=\"font-size: 27px; font-weight: 700; color: #9031aa\"\r\n            id=\"rujin\"\r\n          ></span>\r\n        </li>\r\n        <li>入库金额</li>\r\n      </ul>\r\n    </div>\r\n    <div id=\"sandd1\">\r\n      <ul id=\"kcsll1\">\r\n        <li>\r\n          <span\r\n            style=\"font-size: 27px; font-weight: 700; color: #538ff7\"\r\n            id=\"cu\"\r\n          ></span>\r\n        </li>\r\n        <li>出库数量</li>\r\n      </ul>\r\n      <ul id=\"kcslll1\">\r\n        <li>\r\n          <span\r\n            style=\"font-size: 27px; font-weight: 700; color: #538ff7\"\r\n            id=\"cujin\"\r\n          ></span>\r\n        </li>\r\n        <li>出库金额</li>\r\n      </ul>\r\n    </div>\r\n    <div style=\"margin-top: 50px; margin-left: 30px\">\r\n      <el-button id=\"dcc\" @click=\"dcc()\" icon=\"el-icon-download\"\r\n        >导出</el-button\r\n      >\r\n      <el-button id=\"sxx\" @click=\"sxx()\" icon=\"el-icon-refresh\">刷新</el-button>\r\n      <el-table\r\n        tooltip-effect=\"dark\"\r\n        @selection-change=\"handleSelectionChange\"\r\n        height=\"300px\"\r\n        :data=\"tableData\"\r\n        :header-cell-style=\"{\r\n          background: '#F8F8F9',\r\n          color: '#606266',\r\n          'text-align': 'center',\r\n        }\"\r\n        :cell-style=\"{ 'text-align': 'center' }\"\r\n        id=\"kct\"\r\n      >\r\n        <el-table-column type=\"selection\" width=\"55\"> </el-table-column>\r\n        <el-table-column fixed label=\"序号\" type=\"index\" width=\"50\">\r\n        </el-table-column>\r\n        <el-table-column fixed prop=\"inbill_bh\" label=\"出入库单号\" width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column fixed prop=\"\" label=\"出入库日期\" width=\"150\">\r\n          <template slot-scope=\"scope\">\r\n            <span>{{ filterTime(scope.row.inbll_date) }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"业务类型\" width=\"100\">\r\n          <template slot-scope=\"scope\">\r\n            <span v-if=\"scope.row.out1 == 0\">入库</span>\r\n            <span v-else>出库</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"inbill_type\" label=\"出入库类型\" width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"ware_name\" label=\"仓库名称\" width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"inbill_num\" label=\"入库数量\" width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"outbill_num\" label=\"出库数量\" width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"zong\" label=\"入库金额\" width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"czong\" label=\"出库金额\" width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"inbill_duty_man\" label=\"经办人\" width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"create_by\" label=\"制单人\" width=\"100\">\r\n        </el-table-column>\r\n        <el-table-column prop=\"\" label=\"制单时间\" width=\"150\">\r\n          <template slot-scope=\"scope\">\r\n            <span>{{ filterTime(scope.row.create_time) }}</span>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!--分页-->\r\n      <!-- <el-pagination\r\n        id=\"fenyels1\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"currentPage\"\r\n        :page-sizes=\"[6, 12, 18, 24]\"\r\n        :page-size=\"pagesize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"rows\"\r\n      >\r\n      </el-pagination> -->\r\n    </div>\r\n    <div id=\"notification-container\">\r\n      <div id=\"dibu\">\r\n        <el-button id=\"bcccc\" @click=\"gb()\">关闭</el-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport $http from \"../http/base.js\";\r\nexport function formatDate(date) {\r\n  let time = new Date(date);\r\n\r\n  let year = time.getFullYear();\r\n\r\n  let month = time.getMonth() + 1;\r\n  month = month.toString().padStart(2, \"0\");\r\n  let day = time.getDate().toString().padStart(2, \"0\");\r\n  return year + \"年\" + month + \"月\" + day + \"日\";\r\n}\r\nexport default {\r\n  data() {\r\n    return {\r\n      countryList: [],\r\n      countryList2: [],\r\n      formInline: {\r\n        hpname: \"\",\r\n        hptype: \"\",\r\n        lx: \"\",\r\n        ckname: \"\",\r\n        rq: \"\",\r\n      },\r\n      ff: {\r\n        name: \"\",\r\n        bh: \"\",\r\n        lx: \"\",\r\n        gg: \"\",\r\n        dw: \"\",\r\n      },\r\n      tableData: [],\r\n\r\n      multipleSelection: [],\r\n      str: \"1=1\",\r\n\r\n      currentPage: 1, //当前页\r\n      pagesize: 600, //每页显示数\r\n      rows: 0, //总行数\r\n    };\r\n  },\r\n  methods: {\r\n    gb() {\r\n      this.$router.go(-1);\r\n    },\r\n    //保留两位小数好\r\n    formatMoney(value) {\r\n      return value.toFixed(2);\r\n    },\r\n    //仓库下拉框\r\n    CK() {\r\n      var t = this;\r\n      $http\r\n        .get(\"http://localhost:5149/api/Bs_warehouse/Bs_warehouseSelect1\")\r\n        .then((res) => {\r\n          t.countryList = res;\r\n        })\r\n        .catch((err) => {\r\n          alert(err);\r\n        });\r\n      $http\r\n        .get(\"http://localhost:5149/api/Wh_transfer_h/CRLX\")\r\n        .then((res) => {\r\n          t.countryList2 = res;\r\n        })\r\n        .catch((err) => {\r\n          alert(err);\r\n        });\r\n    },\r\n    //查询\r\n    find() {\r\n      var bh = this.$route.query.bh;\r\n      $http\r\n        .get(\"http://localhost:5149/api/Wh_transfer_h/KCX?bh=\" + bh)\r\n        .then((res) => {\r\n          this.ff.name = res.item_name;\r\n          this.ff.bh = res.item_code;\r\n          this.ff.lx = res.item_type_name;\r\n          this.ff.dw = res.unit;\r\n          this.ff.gg = res.model_code;\r\n        })\r\n        .catch((err) => {\r\n          alert(err);\r\n        });\r\n    },\r\n    //每页显示数改变事件\r\n    handleSizeChange(size) {\r\n      this.pagesize = size;\r\n      this.currentPage = 1;\r\n      this.find1();\r\n    },\r\n    //页码改变事件\r\n    handleCurrentChange(page) {\r\n      this.currentPage = page;\r\n      this.find1();\r\n    },\r\n    find1() {\r\n      var bh = this.$route.query.bh;\r\n      this.str += \"and item_code='\" + bh + \"'\";\r\n      $http\r\n        .post(\r\n          \"http://localhost:5149/api/Wh_transfer_h/LiuFen?page=\" +\r\n            this.pagesize +\r\n            \"&pageSize=\" +\r\n            this.currentPage +\r\n            \"&str=\" +\r\n            this.str\r\n        )\r\n        .then((res) => {\r\n          this.tableData = res.list;\r\n          this.rows = res.rows;\r\n          var ru = 0;\r\n          var rujin = 0;\r\n          var cu = 0;\r\n          var cujin = 0;\r\n          for (let i = 0; i < this.tableData.length; i++) {\r\n            ru += 