{"ast":null,"code":"import $http from \"../http/base.js\";\nvar tj = \"1 = 1 and is_del = '0'\";\nexport default {\n  data() {\n    return {\n      tableloading: false,\n      //加载\n      showDefaultAttr: true,\n      multipleSelection: [],\n      // 选中的行数据\n      formInline: {\n        ck: '',\n        zt: '',\n        ren: ''\n      },\n      formInline1: {\n        bh: '',\n        name: '',\n        rl: '',\n        zg: '',\n        dh: '',\n        px: '',\n        bz: '',\n        id: 0,\n        bz: '',\n        mc: ''\n      },\n      formInline2: {\n        bh: '',\n        name: '',\n        rl: '',\n        zg: '',\n        dh: '',\n        px: '',\n        bz: '',\n        bz: '',\n        mc: ''\n      },\n      tableData: [],\n      formLabelWidth: '100px',\n      drawer: false,\n      drawer1: false,\n      direction: 'rtl',\n      radio: '0',\n      radio1: '0',\n      List: [],\n      countryList: [],\n      countryList1: [],\n      bl: '1',\n      currentPage: 1,\n      //当前页\n      pagesize: 3,\n      //每页显示数\n      rows: 0,\n      //总行数\n      tj\n    };\n  },\n  methods: {\n    //查询方法\n    find() {\n      var t = this;\n      $http.post(\"http://localhost:5149/api/Bs_ware_area/FenYeWareArea?currentPage=\" + t.currentPage + \"&pageSize=\" + t.pagesize + \"&str=\" + tj + \"\").then(res => {\n        t.tableData = res.list;\n        t.rows = res.rows;\n        this.czr();\n        this.CK();\n        this.CK1();\n        console.log(res);\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    czr() {\n      $http.post(\"http://localhost:5149/api/SupplyView/Czr\").then(res => {\n        this.List = res;\n        console.log(res);\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    insert() {\n      var t = this;\n      var uu = localStorage.getItem(\"name\");\n      $http.get('http://localhost:5149/api/Bs_ware_area/AreaName?name=' + t.formInline2.name).then(res => {\n        if (res == 0) {\n          $http.post('http://localhost:5149/api/Bs_ware_area/InsertArea?code=' + t.formInline2.bh + '&name=' + t.formInline2.name + '&cap=' + t.formInline2.rl + '&man=' + t.formInline2.zg + '&px=' + t.formInline2.px + '&zt=' + t.radio + '&cz=' + uu + '&gx=' + uu + '&bz=' + t.formInline2.bz + '&dh=' + t.formInline2.dh + '&cc=' + t.formInline2.mc + '').then(res => {\n            if (res) {\n              this.$message({\n                message: '添加成功！',\n                type: 'success'\n              });\n              t.formInline2.bh = \"\";\n              t.formInline2.name = \"\";\n              t.formInline2.rl = \"\";\n              t.formInline2.zg = \"\";\n              t.formInline2.dh = \"\";\n              t.radio = '0';\n              t.formInline2.px = \"\";\n              t.formInline2.bz = \"\";\n              t.formInline2.mc = \"\";\n              t.drawer = false;\n              t.find();\n            }\n          }).catch(err => {\n            console.log(err);\n          });\n        } else {\n          this.$message({\n            message: '库位名称已存在！',\n            type: 'error'\n          });\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    //重置\n    reset() {\n      this.formInline.ck = \"\";\n      this.formInline.zt = \"\";\n      this.formInline.ren = \"\";\n      tj = \"1=1\";\n      this.find();\n    },\n    xg() {\n      var t = this;\n      var uu = localStorage.getItem(\"name\");\n      $http.put('http://localhost:5149/api/Bs_ware_area/UpdateArea?code=' + t.formInline1.bh + '&name=' + t.formInline1.name + '&cap=' + t.formInline1.rl + '&man=' + t.formInline1.zg + '&px=' + t.formInline1.px + '&zt=' + t.radio1 + '&gx=' + uu + '&bz=' + t.formInline1.bz + '&dh=' + t.formInline1.dh + '&id=' + t.formInline1.id + '&cc=' + t.formInline1.cc + '').then(res => {\n        if (res) {\n          this.$message({\n            message: '修改成功！',\n            type: 'success'\n          });\n          t.find();\n          t.drawer1 = false;\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    handleEdit() {\n      this.drawer = true;\n    },\n    Update(id) {\n      var t = this;\n      t.drawer1 = true;\n      $http.get('http://localhost:5149/api/Bs_ware_area/SelectID?id=' + id).then(res => {\n        t.formInline1.bh = res.area_code;\n        t.formInline1.name = res.area_name;\n        t.formInline1.rl = res.area_cap;\n        t.formInline1.zg = res.area_duty_man;\n        t.formInline1.dh = res.dh;\n        t.radio1 = res.area_state;\n        t.formInline1.px = res.sort_code;\n        t.formInline1.bz = res.remark;\n        t.formInline1.id = res.ware_area_id;\n        t.formInline1.mc = res.warehouse_id;\n        console.log(res);\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    //每页显示数改变事件\n    handleSizeChange(size) {\n      this.pagesize = size;\n      this.currentPage = 1;\n      this.find();\n      console.log(`每页 ${size} 条`);\n    },\n    //页码改变事件\n    handleCurrentChange(page) {\n      this.currentPage = page;\n      this.find();\n      console.log(`当前页: ${page}`);\n    },\n    //选择\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n    //删除\n    handleDelete(i) {\n      this.$confirm('此操作将永久删除该信息, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        console.log(i.item_type_id);\n        console.log(this.bl);\n        $http.post(\"http://localhost:5149/api/WareArea/AreaDelete?id=\" + i.ware_area_id + \"&str=\" + this.bl).then(res => {\n          if (res) {\n            this.$message({\n              message: '删除成功！',\n              type: 'success'\n            });\n            this.find();\n          }\n        }).catch(err => {\n          console.log(err);\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'error',\n          message: '已取消删除'\n        });\n      });\n    },\n    //刷新table\n    flushed() {\n      this.tableloading = true;\n      // 模拟获取接口数据\n      setTimeout(() => {\n        //调用find();this.find();\n        this.tableloading = false;\n        this.find();\n      }, 1000);\n      //直接this.查询方法();\n    },\n\n    //批量删除\n    delAll() {\n      this.$confirm('此操作将永久删除该信息, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        const length = this.multipleSelection.length;\n        var ss = [];\n        for (let i = 0; i < this.multipleSelection.length; i++) {\n          ss.push(this.multipleSelection[i].ware_area_id);\n        }\n        console.log('lo' + length);\n        $http.put('http://localhost:5149/api/WareArea/PLDeleteArea', ss).then(res => {\n          if (res) {\n            this.$message({\n              message: '删除成功！',\n              type: 'success'\n            });\n            this.find();\n          }\n        }).catch(err => {\n          console.log(err);\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'error',\n          message: '已取消删除'\n        });\n      });\n    },\n    handleClose(done) {\n      this.$confirm('确认关闭？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(_ => {\n        done();\n      }).catch(_ => {});\n    },\n    search() {\n      tj = \"1=1\";\n      if (this.formInline.ck != \"\") {\n        tj += \" and area_name like '%\" + this.formInline.ck + \"%' or area_code like '%\" + this.formInline.ck + \"%'\";\n      }\n      if (this.formInline.zt != \"\") {\n        tj += \" and area_state like '%\" + this.formInline.zt + \"%'\";\n      }\n      if (this.formInline.ren != \"\") {\n        tj += \" and area_duty_man like '%\" + this.formInline.ren + \"%'\";\n      }\n      this.find();\n    },\n    //仓库下拉框\n    CK() {\n      var t = this;\n      $http.get(\"http://localhost:5149/api/Bs_warehouse/Bs_warehouseSelect1\").then(res => {\n        t.countryList = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    //仓库下拉框\n    CK1() {\n      var t = this;\n      $http.get(\"http://localhost:5149/api/Bs_warehouse/Bs_warehouseSelect1\").then(res => {\n        t.countryList1 = res;\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    filterTime(timestamp) {\n      return formatDate(timestamp);\n    }\n  },\n  mounted() {\n    this.tableloading = true;\n    // 模拟获取接口数据\n    setTimeout(() => {\n      //调用find();\n      this.find();\n      this.tableloading = false;\n    }, 500);\n  }\n};\nexport function formatDate(date) {\n  let time = new Date(date);\n  let year = time.getFullYear();\n  let month = time.getMonth() + 1;\n  month = month.toString().padStart(2, \"0\");\n  let day = time.getDate().toString().padStart(2, \"0\");\n  return year + '年' + month + '月' + day + '日';\n}","map":{"version":3,"names":["$http","tj","data","tableloading","showDefaultAttr","multipleSelection","formInline","ck","zt","ren","formInline1","bh","name","rl","zg","dh","px","bz","id","mc","formInline2","tableData","formLabelWidth","drawer","drawer1","direction","radio","radio1","List","countryList","countryList1","bl","currentPage","pagesize","rows","methods","find","t","post","then","res","list","czr","CK","CK1","console","log","catch","err","alert","insert","uu","localStorage","getItem","get","$message","message","type","reset","xg","put","cc","handleEdit","Update","area_code","area_name","area_cap","area_duty_man","area_state","sort_code","remark","ware_area_id","warehouse_id","handleSizeChange","size","handleCurrentChange","page","handleSelectionChange","val","handleDelete","i","$confirm","confirmButtonText","cancelButtonText","item_type_id","flushed","setTimeout","delAll","length","ss","push","handleClose","done","_","search","filterTime","timestamp","formatDate","mounted","date","time","Date","year","getFullYear","month","getMonth","toString","padStart","day","getDate"],"sources":["src/components/WareArea.vue"],"sourcesContent":["<template>\r\n    <div id=\"kj\">\r\n        <!--头部-->\r\n        <el-breadcrumb separator=\"/\" id=\"card\">\r\n            <el-breadcrumb-item :to=\"{ path: '/Home' }\">首页</el-breadcrumb-item>\r\n            <el-breadcrumb-item><a href=\"javascript:void(0)\">仓库管理</a></el-breadcrumb-item>\r\n            <el-breadcrumb-item>库位管理</el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n        <!--搜索-->\r\n\r\n        <transition-group name=\"lyric\">\r\n\r\n            <div id=\"form\" v-if=\"showDefaultAttr\" key=\"1\">\r\n                <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n                    <el-form-item label=\"库位\" id=\"form_top\">\r\n                        <el-input v-model=\"formInline.ck\" id=\"tex\" placeholder=\"请输入库位名称/编号\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"库位状态\" id=\"form_top\">\r\n                        <el-select v-model=\"formInline.zt\" id=\"tex\" placeholder=\"请选择\">\r\n                            <el-option label=\"启用\" value=\"0\"></el-option>\r\n                            <el-option label=\"停用\" value=\"1\"></el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"仓位主管\" id=\"form_top\" prop=\"ren\">\r\n                        <el-select v-model=\"formInline.ren\" id=\"tex\" placeholder=\"请输入\">\r\n                            <el-option v-for=\"item in List\" :key=\"item.u_id\" :label=\"item.u_True_name\"\r\n                                :value=\"item.u_id\"></el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item id=\"w\">\r\n                        <el-button @click=\"search()\" type=\"primary\" id=\"tex1\" icon=\"el-icon-search\">搜索</el-button>\r\n                        <el-button @click=\"reset()\" id=\"tex2\" icon=\"el-icon-refresh\">重置</el-button>\r\n                        <!-- <el-button type=\"primary\" @click=\"search\">高级搜索</el-button> -->\r\n                    </el-form-item>\r\n                </el-form>\r\n            </div>\r\n        </transition-group>\r\n\r\n        <!--按钮-->\r\n        <div id=\"an\">\r\n            <el-button id=\"tex3\" icon=\"el-icon-plus\" @click=\"handleEdit()\">新增</el-button>\r\n            <!-- <el-button  id=\"tex4\"  icon=\"el-icon-edit\">修改</el-button> -->\r\n            <el-button id=\"tex5\" @click=\"delAll()\" icon=\"el-icon-delete\"\r\n                :disabled=\"multipleSelection.length == 0\">删除</el-button>\r\n            <el-button id=\"tex7\" icon=\"el-icon-upload2\">导入</el-button>\r\n            <el-button id=\"tex6\" icon=\"el-icon-download\">导出</el-button>\r\n        </div>\r\n        <!--隐藏-->\r\n        <div id=\"an1\">\r\n            <el-button icon=\"el-icon-search\" circle @click=\"showDefaultAttr = !showDefaultAttr\" title=\"展开\"></el-button>\r\n            <el-button icon=\"el-icon-refresh\" circle @click=\"flushed\" title=\"刷新\"></el-button>\r\n        </div>\r\n        <!-- 表单 -->\r\n        <el-table tooltip-effect=\"dark\" v-loading=\"tableloading\" @selection-change=\"handleSelectionChange\" :data=\"tableData\"\r\n            :header-cell-style=\"{ background: '#F8F8F9', color: '#606266' }\" id=\"tableMain\">\r\n            <el-table-column type=\"selection\" width=\"55\">\r\n            </el-table-column>\r\n            <el-table-column fixed prop=\"ware_area_id\" label=\"序号\" width=\"60\">\r\n            </el-table-column>\r\n            <el-table-column fixed prop=\"area_code\" label=\"库位编号\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column fixed prop=\"area_name\" label=\"库位名称\" width=\"100\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"area_cap\" label=\"库位容量/立方\" width=\"120\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"area_duty_man\" label=\"仓位主管\" width=\"90\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"sort_code\" label=\"排序\" width=\"60\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"area_state\" label=\"库位状态\" width=\"90\">\r\n                <template slot-scope=\"scope\">\r\n                    <span v-if=\"scope.row.area_state == 0\" style=\"color: blue;font-weight: 600;font-size: 13px;\">启用</span>\r\n                    <span v-if=\"scope.row.area_state == 1\" style=\"color: red;font-weight: 600;font-size: 13px;\">停用</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"remark\" label=\"备注\" width=\"160\">\r\n            </el-table-column>\r\n            <el-table-column label=\"操作\" fixed=\"right\" width=\"100\">\r\n                <template slot-scope=\"scope\">\r\n                    <img src=\"../Images/bianji1.png\" title=\"编辑\" width=\"25px\" height=\"25px\"\r\n                        @click=\"Update(scope.row.ware_area_id)\">\r\n                    <img src=\"../Images/shanchu.png\" style=\"margin-left: 10px;\" title=\"删除\" width=\"28px\" height=\"28px\"\r\n                        @click=\"handleDelete(scope.row)\">\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n\r\n        <!-- 分页 -->\r\n        <el-pagination id=\"fenye\" @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\"\r\n            :current-page=\"currentPage\" :page-sizes=\"[3, 6, 9, 12]\" :page-size=\"pagesize\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\" :total=\"rows\">\r\n        </el-pagination>\r\n        <el-drawer title=\"新增\" :visible.sync=\"drawer\" :direction=\"direction\" :before-close=\"handleClose\">\r\n            <div class=\"demo-drawer__content\">\r\n                <el-form :model=\"formInline2\">\r\n                    <el-form-item label=\"库位编号\" :label-width=\"formLabelWidth\">\r\n                        <el-input v-model=\"formInline2.bh\" autocomplete=\"off\" placeholder=\"自动获取系统编号\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"库位名称\" :label-width=\"formLabelWidth\">\r\n                        <el-input v-model=\"formInline2.name\" autocomplete=\"off\" placeholder=\"请输入\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"父级仓库\" :label-width=\"formLabelWidth\">\r\n                    <el-select v-model=\"formInline2.mc\" id=\"tex\" clearable filterable placeholder=\"请选择\">\r\n                                <el-option\r\n                            v-for=\"item in countryList\"\r\n                            :key=\"item.warehouse_id\"\r\n                            :label=\"item.ware_name\"\r\n                            :value=\"item.ware_name\"\r\n                          ></el-option>\r\n                            </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"库位容量/立方\" :label-width=\"formLabelWidth\">\r\n                        <el-input v-model=\"formInline2.rl\" autocomplete=\"off\" type=\"number\" placeholder=\"请输入\"></el-input>\r\n                        <p v-if=\"isNaN(formInline2.rl)\">必须输入数字</p>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"仓位主管\" :label-width=\"formLabelWidth\">\r\n                        <el-select v-model=\"formInline2.zg\" id=\"tex\" placeholder=\"请输入\">\r\n                            <el-option v-for=\"item in List\" :key=\"item.u_id\" :label=\"item.u_True_name\"\r\n                                :value=\"item.u_id\"></el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"联系电话\" :label-width=\"formLabelWidth\">\r\n                        <el-input v-model=\"formInline2.dh\" autocomplete=\"off\" type=\"number\" placeholder=\"请输入\"></el-input>\r\n                        <p v-if=\"isNaN(formInline2.dh)\">必须输入数字</p>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"库位状态\" :label-width=\"formLabelWidth\">\r\n                        <el-radio v-model=\"radio\" label=\"0\">启用</el-radio>\r\n                        <el-radio v-model=\"radio\" label=\"1\">停用</el-radio>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"排序\" :label-width=\"formLabelWidth\">\r\n                        <el-input v-model=\"formInline2.px\" autocomplete=\"off\" type=\"number\" placeholder=\"请输入\"></el-input>\r\n                        <p v-if=\"isNaN(formInline2.px)\">必须输入数字</p>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"备注\" :label-width=\"formLabelWidth\">\r\n                        <el-input v-model=\"formInline2.bz\" autocomplete=\"off\" placeholder=\"请输入\"></el-input>\r\n                    </el-form-item>\r\n                </el-form>\r\n                <div class=\"demo-drawer__footer\" id=\"didi\">\r\n                    <el-button id=\"tj\" @click=\"insert()\">提交</el-button>\r\n                    <el-button @click=\"drawer = false\" id=\"qx\">取 消</el-button>\r\n                </div>\r\n            </div>\r\n        </el-drawer>\r\n        <el-drawer title=\"修改\" :visible.sync=\"drawer1\" :direction=\"direction\" :before-close=\"handleClose\">\r\n            <div class=\"demo-drawer__content\">\r\n                <el-form :model=\"formInline1\">\r\n                    <el-form-item label=\"库位编号\" :label-width=\"formLabelWidth\">\r\n                        <el-input v-model=\"formInline1.bh\" autocomplete=\"off\" placeholder=\"自动获取系统编号\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"库位名称\" :label-width=\"formLabelWidth\">\r\n                        <el-input v-model=\"formInline1.name\" autocomplete=\"off\" placeholder=\"请输入\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"父级仓库\" :label-width=\"formLabelWidth\">\r\n                    <el-select v-model=\"formInline1.mc\" id=\"tex\" clearable filterable placeholder=\"请选择\">\r\n                                <el-option\r\n                            v-for=\"item in countryList1\"\r\n                            :key=\"item.warehouse_id\"\r\n                            :label=\"item.ware_name\"\r\n                            :value=\"item.ware_name\"\r\n                          ></el-option>\r\n                            </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"库位容量/立方\" :label-width=\"formLabelWidth\">\r\n                        <el-input v-model=\"formInline1.rl\" autocomplete=\"off\" type=\"number\" placeholder=\"请输入\"></el-input>\r\n                        <p v-if=\"isNaN(formInline1.rl)\">必须输入数字</p>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"仓位主管\" :label-width=\"formLabelWidth\">\r\n                        <el-select v-model=\"formInline1.zg\" id=\"tex\" placeholder=\"请输入\">\r\n                            <el-option v-for=\"item in List\" :key=\"item.u_id\" :label=\"item.u_True_name\"\r\n                                :value=\"item.u_id\"></el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"联系电话\" :label-width=\"formLabelWidth\">\r\n                        <el-input v-model=\"formInline1.dh\" autocomplete=\"off\" type=\"number\" placeholder=\"请输入\"></el-input>\r\n                        <p v-if=\"isNaN(formInline1.dh)\">必须输入数字</p>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"库位状态\" :label-width=\"formLabelWidth\">\r\n                        <el-radio v-model=\"radio1\" label=\"0\">启用</el-radio>\r\n                        <el-radio v-model=\"radio1\" label=\"1\">停用</el-radio>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"排序\" :label-width=\"formLabelWidth\">\r\n                        <el-input v-model=\"formInline1.px\" autocomplete=\"off\" type=\"number\" placeholder=\"请输入\"></el-input>\r\n                        <p v-if=\"isNaN(formInline1.px)\">必须输入数字</p>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"备注\" :label-width=\"formLabelWidth\">\r\n                        <el-input v-model=\"formInline1.bz\" autocomplete=\"off\" placeholder=\"请输入\"></el-input>\r\n                    </el-form-item>\r\n                </el-form>\r\n                <div class=\"demo-drawer__footer\" id=\"didi\">\r\n                    <el-button id=\"tj\" @click=\"xg()\">提交</el-button>\r\n                    <el-button @click=\"drawer1 = false\" id=\"qx\">取 消</el-button>\r\n                </div>\r\n            </div>\r\n        </el-drawer>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport $http from \"../http/base.js\";\r\nvar tj = \"1 = 1 and is_del = '0'\";\r\nexport default {\r\n    data() {\r\n        return {\r\n            tableloading: false,//加载\r\n            showDefaultAttr: true,\r\n            multipleSelection: [],// 选中的行数据\r\n            formInline: {\r\n                ck: '',\r\n                zt: '',\r\n                ren: '',\r\n            },\r\n            formInline1: {\r\n                bh: '',\r\n                name: '',\r\n                rl: '',\r\n                zg: '',\r\n                dh: '',\r\n                px: '',\r\n                bz: '',\r\n                id: 0,\r\n                bz: '',\r\n                mc:''\r\n            },\r\n            formInline2: {\r\n                bh: '',\r\n                name: '',\r\n                rl: '',\r\n                zg: '',\r\n                dh: '',\r\n                px: '',\r\n                bz: '',\r\n                bz: '',\r\n                mc:''\r\n            },\r\n            tableData: [],\r\n    