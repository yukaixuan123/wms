ap=' + \r\n                        t.formInline2.rl + '&man=' + \r\n                        t.formInline2.zg+ '&px=' + \r\n                        t.formInline2.px + '&zt=' + \r\n                        t.radio + '&cz=' + \r\n                        uu + '&gx=' + \r\n                        uu + '&bz=' + \r\n                        t.formInline2.bz + '&dh=' + \r\n                        t.formInline2.dh + '&cc='+\r\n                        t.formInline2.mc+'')\r\n                            .then(res => {\r\n                                if (res) {\r\n                                    this.$message({\r\n                                        message: '添加成功！',\r\n                                        type: 'success'\r\n                                    });\r\n                                    t.formInline2.bh = \"\";\r\n                                    t.formInline2.name = \"\";\r\n                                    t.formInline2.rl = \"\";\r\n                                    t.formInline2.zg = \"\";\r\n                                    t.formInline2.dh = \"\";\r\n                                    t.radio = '0';\r\n                                    t.formInline2.px = \"\";\r\n                                    t.formInline2.bz = \"\";\r\n                                    t.formInline2.mc = \"\";\r\n                                    t.drawer = false;\r\n                                    t.find();\r\n                                }\r\n                            })\r\n                            .catch(err => {\r\n                                console.log(err);\r\n                            })\r\n                    } else {\r\n\r\n                        this.$message({\r\n                            message: '库位名称已存在！',\r\n                            type: 'error'\r\n                        });\r\n                    }\r\n                })\r\n                .catch(err => {\r\n                    console.log(err);\r\n             