{"ast":null,"code":"var str = \" 1=1 and is_del=0 \";\nvar fjid = 0;\nimport $http from \"../http/base.js\";\nexport default {\n  data() {\n    return {\n      tableloading: false,\n      //加载\n      showDefaultAttr: true,\n      multipleSelection: [],\n      // 选中的行数据\n      formInline: {\n        bh: \"\",\n        zt: \"\",\n        px: \"\",\n        bz: \"\",\n        name: \"\",\n        father_code: \"\"\n      },\n      formInline1: {\n        fl: \"\",\n        bh: \"\",\n        name: \"\",\n        px: \"\",\n        bz: \"\",\n        zt: \"\",\n        item_type_id: 0,\n        father_code: \"\"\n      },\n      formInline2: {\n        lx: \"\",\n        zt: \"\",\n        ren: \"\",\n        startDate: \"\"\n      },\n      value1: [],\n      options: [],\n      menuItems: [],\n      tableData: [],\n      cateList: [],\n      formLabelWidth: \"100px\",\n      drawer: false,\n      drawer1: false,\n      direction: \"rtl\",\n      radio: \"0\",\n      radio1: \"0\",\n      List: [],\n      bl: \"1\",\n      direction: \"rtl\",\n      currentPage: 1,\n      //当前页\n      pagesize: 10,\n      //每页显示数\n      rows: 0 //总行数\n    };\n  },\n\n  methods: {\n    //查询方法\n    find() {\n      var t = this;\n      // $http.get('http://localhost:5149/api/Bs_itemType/fatherGoodsFind'\n      // ).then(res => {\n      //     console.log(res);\n      //     this.tableData = res.label;\n      // })\n      $http.get(\" http://localhost:5149/api/Bs_itemType/FindGoodsType\", {\n        params: {\n          currentPage: this.currentPage,\n          pageSize: this.pagesize,\n          str\n        }\n      }).then(res => {\n        console.log(res);\n        this.cateList = res.list;\n        this.rows = res.rows;\n      });\n    },\n    cascader() {\n      $http.get(\"http://localhost:5149/api/Sys_encode/GetItems?father_code=0\").then(res => {\n        console.log(res);\n        this.menuItems = res;\n        this.options = this.transformMenuItems(this.menuItems);\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    transformMenuItems(menuItems) {\n      //级联方法\n      const op = [];\n      for (const item of menuItems) {\n        const option = {\n          value: item.item_type_code,\n          label: item.item_type_name\n        };\n        if (item.children && item.children.length > 0) {\n          option.children = this.transformMenuItems(item.children);\n        }\n        op.push(option);\n      }\n      return op;\n    },\n    czr() {\n      // $http\n      //   .get(\"http://localhost:5149/api/Bs_itemType/fatherGoodsFind\")\n      //   .then((res) => {\n      //     console.log(res);\n      //     this.options = res;\n      //   });\n    },\n    add() {\n      this.drawer = true;\n      $http.get(\"http://localhost:5149/api/Sys_encode/FindSys_encodeById?id=1\").then(res => {\n        this.formInline.bh = res;\n      });\n    },\n    closeDrawer() {\n      var sc;\n      if (this.formInline.father_code && this.formInline.father_code.length > 0) {\n        // Set to the last selected value (assuming checkStrictly is true)\n        sc = this.formInline.father_code[this.formInline.father_code.length - 1];\n      } else {\n        // Nothing selected, set to default value (e.g., \"0\")\n        sc = \"0\";\n      }\n      console.log(sc);\n      var name = localStorage.getItem(\"b\");\n      $http.post(\"http://localhost:5149/api/Bs_itemType/AddGoodsType?father_code=\" + sc + \"&item_type_code=\" + this.formInline.bh + \"&item_type_name=\" + this.formInline.name + \"&inbill_state=\" + this.formInline.zt + \"&sort_code=\" + this.formInline.px + \"&remark=\" + this.formInline.bz + \"&create_by=\" + name + \"&is_del=0\").then(res => {\n        if (res) {\n          this.$message({\n            message: \"新增成功！\",\n            type: \"success\"\n          });\n          this.drawer = false;\n          this.find();\n        }\n        console.log(res);\n      });\n    },\n    //重置\n    reset() {\n      this.formInline2.lx = \"\";\n      this.formInline2.zt = \"\";\n      this.formInline2.ren = \"\";\n      this.formInline2.startDate = \"\";\n      this.find();\n    },\n    handleEdit(obj) {\n      this.drawer1 = true;\n      console.log(obj);\n      this.formInline1.bh = obj.item_type_code;\n      this.formInline1.name = obj.item_type_name;\n      this.formInline1.zt = obj.inbill_state;\n      this.formInline1.px = obj.sort_code;\n      this.formInline1.bz = obj.remark;\n      this.formInline1.father_code = obj.father_code;\n      this.formInline1.item_type_id = obj.item_type_id;\n    },\n    updateok() {\n      // var h = 0;\n      // if (this.value.length > 0) {\n      //   h = parseInt(this.value[this.value.length - 1]);\n      // } else {\n      //   h = this.value;\n      // }\n      var ppid = \"\";\n      if (this.formInline1.father_code.slice(-1)[0].length == 1) {\n        ppid = this.formInline1.father_code;\n      } else {\n        ppid = this.formInline1.father_code.slice(-1)[0];\n      }\n      var name = localStorage.getItem(\"b\");\n      $http.put(\"http://localhost:5149/api/Bs_itemType/UpdateGoodsType?father_code=\" + ppid + \"&item_type_code=\" + this.formInline1.bh + \"&item_type_name=\" + this.formInline1.name + \"&inbill_state=\" + this.formInline1.zt + \"&sort_code=\" + this.formInline1.px + \"&remark=\" + this.formInline1.bz + \"&update_by=\" + name + \"&item_type_id=\" + this.formInline1.item_type_id + \"\").then(res => {\n        if (res) {\n          this.$message({\n            message: \"修改成功！\",\n            type: \"success\"\n          });\n          this.drawer1 = false;\n          this.find();\n        }\n      });\n    },\n    // update(obj) {\n    //     this.drawer1 = true;\n    //    console.log(obj);\n    // },\n    //搜索\n    search() {\n      this.currentPage = 1;\n      str = \" 1=1  is_del=0 \";\n      if (this.formInline2.lx != \"\") {\n        str += \" AND item_type_code like CONCAT('%','\" + this.formInline2.lx + \"','%') or  item_type_name like CONCAT('%','\" + this.formInline2.lx + \"','%')\";\n      }\n      if (this.formInline2.ren != \"\") {\n        str += \" AND create_by like CONCAT('%','\" + this.formInline2.ren + \"','%')\";\n      }\n      if (this.formInline2.zt != \"\") {\n        str += \" AND inbill_state like CONCAT('%','\" + this.formInline2.zt + \"','%')\";\n      }\n      // if (this.formInline2.startDate != \"\") {\n      //     str += \" AND create_time like CONCAT('%','\" + this.formInline2.startDate + \"','%')\";\n      // }\n      if (this.formInline2.startDate != \"\") {\n        const yearMonthDay = this.formInline2.startDate.substring(0, 10);\n        str += \"and convert(varchar(50),create_time,120) LIKE  '\" + yearMonthDay + \"%'\";\n      }\n      this.find();\n    },\n    //选择\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n    //删除\n    handleDelete(i) {\n      this.$confirm(\"此操作将永久删除该信息, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        $http.get(\"http://localhost:5149/api/Sys_encode/FindBs_item_type?item_type_code=\" + i.item_type_code).then(res => {\n          if (res && res.length > 0) {\n            this.$message({\n              type: \"error\",\n              message: \"该主表被子表索引\"\n            });\n          } else {\n            // console.log(i.father_code);\n            // $http.get('http://localhost:5149/api/Sys_encode/FindFather11?father_code=' + i.father_code).then(res => {\n            //   console.log(res);\n            // })\n            $http.put(\"http://localhost:5149/api/Sys_encode/DeleteBs_item_type?item_type_code=\" + i.item_type_code).then(res => {\n              this.$message({\n                type: \"success\",\n                message: \"删除成功！\"\n              });\n              this.find();\n            });\n          }\n        });\n      }).catch(() => {\n        this.$message({\n          type: \"error\",\n          message: \"已取消删除\"\n        });\n      });\n    },\n    //每页显示数改变事件\n    handleSizeChange(size) {\n      this.pagesize = size;\n      this.currentPage = 1;\n      this.find();\n    },\n    //页码改变事件\n    handleCurrentChange(page) {\n      this.currentPage = page;\n      this.find();\n    },\n    //刷新table\n    flushed() {\n      this.tableloading = true;\n      // 模拟获取接口数据\n      setTimeout(() => {\n        //调用find();this.find();\n        this.tableloading = false;\n        this.find();\n      }, 1000);\n      //直接this.查询方法();\n    },\n\n    //批量删除\n    delAll() {\n      this.$confirm(\"此操作将永久删除该信息, 是否继续?\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(() => {\n        const length = this.multipleSelection.length;\n        var ss = [];\n        for (let i = 0; i < this.multipleSelection.length; i++) {\n          ss.push(this.multipleSelection[i].item_type_id);\n        }\n        console.log(\"lo\" + length);\n        $http.put(\"http://localhost:5149/api/Bs_itemType/PLDeleteItemType\", ss).then(res => {\n          if (res) {\n            this.$message({\n              message: \"删除成功！\",\n              type: \"success\"\n            });\n            this.find();\n          }\n        }).catch(err => {\n          console.log(err);\n        });\n      }).catch(() => {\n        this.$message({\n          type: \"error\",\n          message: \"已取消删除\"\n        });\n      });\n    },\n    handleClose(done) {\n      this.$confirm(\"确认关闭？\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n        type: \"warning\"\n      }).then(_ => {\n        done();\n      }).catch(_ => {});\n    },\n    handleChange(value) {\n      console.log(value);\n    },\n    filterTime(timestamp) {\n      return formatDate(timestamp);\n    },\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    }\n  },\n  mounted() {\n    this.find();\n    this.cascader();\n    this.tableloading = true;\n    // 模拟获取接口数据\n    setTimeout(() => {\n      //调用find();\n      this.find();\n      this.tableloading = false;\n    }, 500);\n  }\n};\nexport function formatDate(date) {\n  let time = new Date(date);\n  let year = time.getFullYear();\n  let month = time.getMonth() + 1;\n  month = month.toString().padStart(2, \"0\");\n  let day = time.getDate().toString().padStart(2, \"0\");\n  return year + \"年\" + month + \"月\" + day + \"日\";\n}","map":{"version":3,"names":["str","fjid","$http","data","tableloading","showDefaultAttr","multipleSelection","formInline","bh","zt","px","bz","name","father_code","formInline1","fl","item_type_id","formInline2","lx","ren","startDate","value1","options","menuItems","tableData","cateList","formLabelWidth","drawer","drawer1","direction","radio","radio1","List","bl","currentPage","pagesize","rows","methods","find","t","get","params","pageSize","then","res","console","log","list","cascader","transformMenuItems","catch","err","op","item","option","value","item_type_code","label","item_type_name","children","length","push","czr","add","closeDrawer","sc","localStorage","getItem","post","$message","message","type","reset","handleEdit","obj","inbill_state","sort_code","remark","updateok","ppid","slice","put","search","yearMonthDay","substring","handleSelectionChange","val","handleDelete","i","$confirm","confirmButtonText","cancelButtonText","handleSizeChange","size","handleCurrentChange","page","flushed","setTimeout","delAll","ss","handleClose","done","_","handleChange","filterTime","timestamp","formatDate","mounted","date","time","Date","year","getFullYear","month","getMonth","toString","padStart","day","getDate"],"sources":["src/components/GoodsType.vue"],"sourcesContent":["<template>\n  <div id=\"kj\">\n    <!--头部-->\n    <el-breadcrumb separator=\"/\" id=\"card\">\n      <el-breadcrumb-item :to=\"{ path: '/Home' }\">首页</el-breadcrumb-item>\n      <el-breadcrumb-item\n        ><a href=\"javascript:void(0)\">货品管理</a></el-breadcrumb-item\n      >\n      <el-breadcrumb-item>货品类型</el-breadcrumb-item>\n    </el-breadcrumb>\n    <!--搜索-->\n    <transition-group name=\"lyric\">\n      <div id=\"form\" v-if=\"showDefaultAttr\" key=\"1\">\n        <el-form :inline=\"true\" :model=\"formInline2\" class=\"demo-form-inline\">\n          <el-form-item label=\"货品类型\" id=\"form_top\">\n            <el-input\n              v-model=\"formInline2.lx\"\n              id=\"tex\"\n              placeholder=\"请输入货品类型名称/编号\"\n            ></el-input>\n          </el-form-item>\n          <el-form-item label=\"状态\" id=\"form_top\">\n            <el-select v-model=\"formInline2.zt\" id=\"tex\" placeholder=\"请选择\">\n              <el-option label=\"启用\" value=\"0\"></el-option>\n              <el-option label=\"停用\" value=\"1\"></el-option>\n            </el-select>\n          </el-form-item>\n          <el-form-item label=\"操作人\" id=\"form_top\" prop=\"ren\">\n            <!-- <el-select v-model=\"formInline2.ren\" id=\"tex\" placeholder=\"请输入\">\n                            <el-option v-for=\"item in List\" :key=\"item.u_id\" :label=\"item.u_True_name\"\n                                :value=\"item.u_id\"></el-option>\n                        </el-select> -->\n            <el-input\n              v-model=\"formInline2.ren\"\n              id=\"tex\"\n              placeholder=\"操作人\"\n            ></el-input>\n          </el-form-item>\n          <el-form-item label=\"操作时间\" prop=\"startDate\">\n            <el-date-picker\n              id=\"tex\"\n              v-model=\"formInline2.startDate\"\n              value-format=\"yyyy-MM-dd\"\n              clearable\n              type=\"date\"\n              placeholder=\"请选择\"\n            />\n          </el-form-item>\n          <el-form-item id=\"w\">\n            <el-button\n              @click=\"search()\"\n              type=\"primary\"\n              id=\"tex1\"\n              icon=\"el-icon-search\"\n              >搜索</el-button\n            >\n            <el-button @click=\"reset()\" id=\"tex2\" icon=\"el-icon-refresh\"\n              >重置</el-button\n            >\n            <!-- <el-button type=\"primary\" @click=\"search\">高级搜索</el-button> -->\n          </el-form-item>\n        </el-form>\n      </div>\n    </transition-group>\n\n    <!--按钮-->\n    <div id=\"an\">\n      <el-button id=\"tex3\" icon=\"el-icon-plus\" @click=\"add()\">新增</el-button>\n      <!-- <el-button  id=\"tex4\"  icon=\"el-icon-edit\">修改</el-button> -->\n      <el-button\n        id=\"tex5\"\n        @click=\"delAll()\"\n        icon=\"el-icon-delete\"\n        :disabled=\"multipleSelection.length == 0\"\n        >删除</el-button\n      >\n      <el-button id=\"tex7\" icon=\"el-icon-upload2\">导入</el-button>\n      <el-button id=\"tex6\" icon=\"el-icon-download\">导出</el-button>\n    </div>\n    <!--隐藏-->\n    <div id=\"an1\">\n      <el-button\n        icon=\"el-icon-search\"\n        circle\n        @click=\"showDefaultAttr = !showDefaultAttr\"\n        title=\"展开\"\n      ></el-button>\n      <el-button\n        icon=\"el-icon-refresh\"\n        circle\n        @click=\"flushed\"\n        title=\"刷新\"\n      ></el-button>\n    </div>\n    <!-- 表单 -->\n    <el-table\n      tooltip-effect=\"dark\"\n      height=\"450px\"\n      v-loading=\"tableloading\"\n      @selection-change=\"handleSelectionChange\"\n      :data=\"cateList\"\n      :header-cell-style=\"{ background: '#F8F8F9', color: '#606266' }\"\n      id=\"tableMain\"\n      element-loading-text=\"拼命加载中\"\n      element-loading-spinner=\"el-icon-loading\"\n      row-key=\"item_type_id\"\n    >\n      <el-table-column type=\"selection\" width=\"55\"> </el-table-column>\n      <el-table-column fixed label=\"序号\" type=\"index\" width=\"50\">\n      </el-table-column>\n      <el-table-column\n        fixed\n        prop=\"item_type_name\"\n        label=\"货品类型名称\"\n        width=\"120\"\n      >\n      </el-table-column>\n\n      <el-table-column\n        prop=\"item_type_code\"\n        label=\"货品类型编号\"\n        fixed\n        width=\"120\"\n      >\n      </el-table-column>\n\n      <el-table-column fixed label=\"状态\" width=\"100\">\n        <!-- <template slot-scope=\"scope\">\n          <span\n            v-if=\"scope.row.inbill_state == 0\"\n            style=\"color: blue; font-weight: 600; font-size: 13px\"\n            >正常</span\n          >\n          <span\n            v-if=\"scope.row.inbill_state == 1\"\n            style=\"color: red; font-weight: 600; font-size: 13px\"\n            >停用</span\n          >\n        </template> -->\n        <template slot-scope=\"scope\">\n          <div v-if=\"scope.row.inbill_state == 0\">\n            <el-tag>启用</el-tag>\n          </div>\n          <div v-if=\"scope.row.inbill_state == 1\">\n            <el-tag type=\"danger\">停用 </el-tag>\n          </div>\n        </template>\n      </el-table-column>\n\n      <el-table-column prop=\"sort_code\" label=\"排序\" width=\"120\">\n      </el-table-column>\n      <el-table-column prop=\"remark\" label=\"备注\" width=\"110\">\n      </el-table-column>\n\n      <el-table-column prop=\"create_by\" label=\"操作人\" width=\"120\">\n      </el-table-column>\n      <el-table-column label=\"操作时间\" width=\"150\">\n        <template slot-scope=\"scope\">\n          <span>{{ filterTime(scope.row.create_time) }}</span>\n        </template>\n      </el-table-column>\n\n      <el-table-column label=\"操作\" fixed=\"right\" width=\"100\">\n        <template slot-scope=\"scope\">\n          <img\n            src=\"../Images/bianji1.png\"\n            title=\"编辑\"\n            width=\"25px\"\n            height=\"25px\"\n            @click=\"handleEdit(scope.row)\"\n          />\n          <img\n            src=\"../Images/shanchu.png\"\n            width=\"28px\"\n            height=\"28px\"\n            style=\"margin-left: 5px\"\n            title=\"删除\"\n            @click=\"handleDelete(scope.row)\"\n          />\n        </template>\n      </el-table-column>\n    </el-table>\n    <el-pagination\n      id=\"fenye\"\n      @size-change=\"handleSizeChange\"\n      @current-change=\"handleCurrentChange\"\n      :current-page=\"currentPage\"\n      :page-sizes=\"[10, 20, 30, 40]\"\n      :page-size=\"pagesize\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n      :total=\"rows\"\n    >\n    </el-pagination>\n    <!-- 新增 -->\n    <el-drawer\n      title=\"新增\"\n      :visible.sync=\"drawer\"\n      :direction=\"direction\"\n      :before-close=\"handleClose\"\n    >\n      <div class=\"demo-drawer__content\">\n        <el-form :model=\"formInline\" id=\"lxtjjj\">\n          <el-form-item label=\"上级分类\" :label-width=\"formLabelWidth\">\n            <el-cascader\n              id=\"hplxbd\"\n              :options=\"options\"\n              v-model=\"formInline.father_code\"\n              :props=\"{ checkStrictly: true }\"\n              @change=\"handleChange\"\n              clearable\n            ></el-cascader>\n          </el-form-item>\n          <el-form-item label=\"类型编号\" :label-width=\"formLabelWidth\">\n            <el-input\n              v-model=\"formInline.bh\"\n              autocomplete=\"off\"\n              id=\"hplxbd\"\n            ></el-input>\n          </el-form-item>\n          <el-form-item label=\"类型名称\" :label-width=\"formLabelWidth\">\n            <el-input\n              v-model=\"formInline.name\"\n              autocomplete=\"off\"\n              id=\"hplxbd\"\n            ></el-input>\n          </el-form-item>\n          <el-form-item label=\"排序\" :label-width=\"formLabelWidth\">\n            <el-input\n              v-model=\"formInline.px\"\n              autocomplete=\"off\"\n              id=\"hplxbd\"\n            ></el-input>\n          </el-form-item>\n          <el-form-item label=\"状态\" :label-width=\"formLabelWidth\">\n            <el-radio v-model=\"formInline.zt\" label=\"0\">启用</el-radio>\n            <el-radio v-model=\"formInline.zt\" label=\"1\">停用</el-radio>\n          </el-form-item>\n          <el-form-item label=\"备注\" :label-width=\"formLabelWidth\">\n            <el-input\n              v-model=\"formInline.bz\"\n              autocomplete=\"off\"\n              id=\"hplxbd\"\n            ></el-input>\n          </el-form-item>\n        </el-form>\n        <div class=\"demo-drawer__footer\" id=\"didi\">\n          <el-button id=\"tj\" @click=\"closeDrawer()\">提交</el-button>\n          <el-button @click=\"drawer = false\" id=\"qx\">取 消</el-button>\n        </div>\n      </div>\n    </el-drawer>\n    <!-- 修改 -->\n\n    <el-drawer\n      title=\"修改\"\n      :visible.sync=\"drawer1\"\n      :direction=\"direction\"\n      :before-close=\"handleClose\"\n    >\n      <div class=\"demo-drawer__content\">\n        <el-form :model=\"formInline1\" id=\"lxtjjj\">\n          <el-form-item label=\"上级分类\" :label-width=\"formLabelWidth\">\n            <el-cascader\n              id=\"hplxbd\"\n              :options=\"options\"\n              v-model=\"formInline1.father_code\"\n              :props=\"{ checkStrictly: true }\"\n              @change=\"handleChange\"\n              clearable\n            ></el-cascader>\n          </el-form-item>\n          <el-form-item label=\"类型编号\" :label-width=\"formLabelWidth\">\n            <el-input\n              v-model=\"formInline1.bh\"\n              autocomplete=\"off\"\n              id=\"hplxbd\"\n            ></el-input>\n          </el-form-item>\n          <el-form-item label=\"类型名称\" :label-width=\"formLabelWidth\">\n            <el-input\n              v-model=\"formInline1.name\"\n              autocomplete=\"off\"\n              id=\"hplxbd\"\n            ></el-input>\n          </el-form-item>\n          <el-form-item label=\"排序\" :label-width=\"formLabelWidth\">\n            <el-input\n              v-model=\"formInline1.px\"\n              autocomplete=\"off\"\n              id=\"hplx